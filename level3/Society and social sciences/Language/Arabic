<th id="mwBxY"><span class="IPA nowrap" title="Representation in the International Phonetic Alphabet (IPA)" about="#mwt713" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"IPAblink","href":"./Template:IPAblink"},"params":{"1":{"wt":"w"}},"i":0}}]}' id="mwBxc"><span typeof="mw:Entity">[</span><a rel="mw:WikiLink" href="./Voiced_labial–velar_approximant" title="Voiced labial–velar approximant" class="mw-redirect">w</a><span typeof="mw:Entity">]</span></span></th>
<td style="text-align: center; font-size: 125%" dir="rtl" id="mwBxg"><big id="mwBxk">و</big></td>
<td id="mwBxo"><i id="mwBxs"><i lang="sem-Latn" title="Semitic languages transliteration" about="#mwt714" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"transl","href":"./Template:Transl"},"params":{"1":{"wt":"sem"},"2":{"wt":"w"}},"i":0}}]}' id="mwBxw">w</i></i></td>
<td colspan="3" id="mwBx0"><span title="Representation in the International Phonetic Alphabet (IPA)" class="IPA" about="#mwt715" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"IPA","href":"./Template:IPA"},"params":{"1":{"wt":"/w/"}},"i":0}}]}' id="mwBx4">/w/</span></td></tr>
