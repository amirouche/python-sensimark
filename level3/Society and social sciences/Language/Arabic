<th id="mwBd0"><i lang="sem-Latn" title="Semitic languages transliteration" about="#mwt598" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"transl","href":"./Template:Transl"},"params":{"1":{"wt":"sem"},"2":{"wt":"[[Kaph|*k]]"}},"i":0}}]}' id="mwBd4"><a rel="mw:WikiLink" href="./Kaph" title="Kaph">*k</a></i></th>
<th id="mwBd8"><span class="IPA nowrap" title="Representation in the International Phonetic Alphabet (IPA)" about="#mwt599" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"IPAblink","href":"./Template:IPAblink"},"params":{"1":{"wt":"k"}},"i":0}}]}' id="mwBeA"><span typeof="mw:Entity">[</span><a rel="mw:WikiLink" href="./Voiceless_velar_stop" title="Voiceless velar stop">k</a><span typeof="mw:Entity">]</span></span></th>
