<td colspan="6" id="mwC04">ḍ</td>
<td id="mwC08">D</td>
<td id="mwC1A">.d</td>
<td id="mwC1E">d/9'</td>
<td id="mwC1I">dh</td></tr>
<tr id="mwC1M">
<th id="mwC1Q"><big id="mwC1U"><span lang="ar" dir="rtl" title="Arabic language text" about="#mwt995" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"rtl-lang","href":"./Template:Rtl-lang"},"params":{"1":{"wt":"ar"},"2":{"wt":"ط"}},"i":0}}]}' id="mwC1Y">ط</span><span typeof="mw:Entity" about="#mwt995">‎</span><link rel="mw:PageProp/Category" href="./Category:Articles_containing_Arabic-language_text" about="#mwt995" id="mwC1c"/></big></th>
<td id="mwC1g"><span class="IPA" about="#mwt996" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"IPA link","href":"./Template:IPA_link"},"params":{"1":{"wt":"ˤ"},"2":{"wt":"tˤ"}},"i":0}}]}' id="mwC1k"><a rel="mw:WikiLink" href="./Pharyngealization" title="Pharyngealization">tˤ</a></span></td>
