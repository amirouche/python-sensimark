<th id="mwC2E"><big id="mwC2I"><span lang="ar" dir="rtl" title="Arabic language text" about="#mwt997" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"rtl-lang","href":"./Template:Rtl-lang"},"params":{"1":{"wt":"ar"},"2":{"wt":"ظ"}},"i":0}}]}' id="mwC2M">ظ</span><span typeof="mw:Entity" about="#mwt997">‎</span><link rel="mw:PageProp/Category" href="./Category:Articles_containing_Arabic-language_text" about="#mwt997" id="mwC2Q"/></big></th>
