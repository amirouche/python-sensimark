<td id="mwCZs"><span title="Representation in the International Phonetic Alphabet (IPA)" class="IPA" about="#mwt726" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"IPA","href":"./Template:IPA"},"params":{"1":{"wt":"p"}},"i":0}}]}' id="mwCZw">p</span><sup id="mwCZ0">3</sup><span typeof="mw:Entity" id="mwCZ4"> </span><span typeof="mw:Entity" id="mwCZ8"> </span><span title="Representation in the International Phonetic Alphabet (IPA)" class="IPA" about="#mwt727" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"IPA","href":"./Template:IPA"},"params":{"1":{"wt":"b"}},"i":0}}]}' id="mwCaA">b</span></td>
<td id="mwCaE"><span title="Representation in the International Phonetic Alphabet (IPA)" class="IPA" about="#mwt728" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"IPA","href":"./Template:IPA"},"params":{"1":{"wt":"t"}},"i":0}}]}' id="mwCaI">t</span><sup id="mwCaM">3</sup><span typeof="mw:Entity" id="mwCaQ"> </span><span typeof="mw:Entity" id="mwCaU"> </span><span title="Representation in the International Phonetic Alphabet (IPA)" class="IPA" about="#mwt729" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"IPA","href":"./Template:IPA"},"params":{"1":{"wt":"d"}},"i":0}}]}' id="mwCaY">d</span></td>
