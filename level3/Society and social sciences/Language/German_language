<th id="mwCcE"><a rel="mw:WikiLink" href="./Fricative_consonant" title="Fricative consonant" id="mwCcI">Fricative</a></th>
<td id="mwCcM"><span title="Representation in the International Phonetic Alphabet (IPA)" class="IPA" about="#mwt736" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"IPA","href":"./Template:IPA"},"params":{"1":{"wt":"f"}},"i":0}}]}' id="mwCcQ">f</span><span typeof="mw:Entity" id="mwCcU"> </span><span typeof="mw:Entity" id="mwCcY"> </span><span title="Representation in the International Phonetic Alphabet (IPA)" class="IPA" about="#mwt737" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"IPA","href":"./Template:IPA"},"params":{"1":{"wt":"v"}},"i":0}}]}' id="mwCcc">v</span></td>
<td id="mwCcg"><span title="Representation in the International Phonetic Alphabet (IPA)" class="IPA" about="#mwt738" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"IPA","href":"./Template:IPA"},"params":{"1":{"wt":"s"}},"i":0}}]}' id="mwCck">s</span><span typeof="mw:Entity" id="mwCco"> </span><span typeof="mw:Entity" id="mwCcs"> </span><span title="Representation in the International Phonetic Alphabet (IPA)" class="IPA" about="#mwt739" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"IPA","href":"./Template:IPA"},"params":{"1":{"wt":"z"}},"i":0}}]}' id="mwCcw">z</span></td>
<td id="mwCc0"><span title="Representation in the International Phonetic Alphabet (IPA)" class="IPA" about="#mwt740" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"IPA","href":"./Template:IPA"},"params":{"1":{"wt":"ʃ"}},"i":0}}]}' id="mwCc4">ʃ</span><span typeof="mw:Entity" id="mwCc8"> </span><span typeof="mw:Entity" id="mwCdA"> </span>(<span title="Representation in the International Phonetic Alphabet (IPA)" class="IPA" about="#mwt741" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"IPA","href":"./Template:IPA"},"params":{"1":{"wt":"ʒ"}},"i":0}}]}' id="mwCdE">ʒ</span>)<sup id="mwCdI">4</sup></td>
<td id="mwCdM"><span title="Representation in the International Phonetic Alphabet (IPA)" class="IPA" about="#mwt742" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"IPA","href":"./Template:IPA"},"params":{"1":{"wt":"x"}},"i":0}}]}' id="mwCdQ">x</span><sup id="mwCdU">1</sup></td>
<td id="mwCdY"><span title="Representation in the International Phonetic Alphabet (IPA)" class="IPA" about="#mwt743" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"IPA","href":"./Template:IPA"},"params":{"1":{"wt":"(ʁ)"}},"i":0}}]}' id="mwCdc">(ʁ)</span><sup id="mwCdg">2</sup></td>
<td id="mwCdk"><span title="Representation in the International Phonetic Alphabet (IPA)" class="IPA" about="#mwt744" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"IPA","href":"./Template:IPA"},"params":{"1":{"wt":"h"}},"i":0}}]}' id="mwCdo">h</span></td></tr>
