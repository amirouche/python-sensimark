<th id="mwAyo"><b id="mwAys"><span title="Proto-Indo-European language" class="Unicode" lang="ine" style="white-space:nowrap;" about="#mwt279" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"PIE","href":"./Template:PIE"},"params":{"1":{"wt":"*p"}},"i":0}}]}' id="mwAyw">*p</span></b></th>
