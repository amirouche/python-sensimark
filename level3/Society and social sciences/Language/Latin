<th id="mwA1A"><span title="Representation in the International Phonetic Alphabet (IPA)" class="IPA" about="#mwt266" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"IPA","href":"./Template:IPA"},"params":{"1":{"wt":"[ɪ]"}},"i":0}}]}' id="mwA1E">[ɪ]</span></th>
