<th scope="col" id="mwBjM">Subjective case</th>
<th scope="col" id="mwBjQ">Objective case</th>
<th scope="col" id="mwBjU">Dependent possessive</th>
<th scope="col" id="mwBjY">Independent possessive</th>
<th scope="col" id="mwBjc">Reflexive</th></tr>
<tr id="mwBjg">
<th scope="row" id="mwBjk">1st p. sg.</th>
<td id="mwBjo"><i id="mwBjs">I</i></td><td id="mwBjw"><i id="mwBj0">me</i></td><td id="mwBj4"><i id="mwBj8">my</i></td><td id="mwBkA"><i id="mwBkE">mine</i></td><td id="mwBkI"><i id="mwBkM">myself</i></td></tr>
<tr id="mwBkQ">
<th scope="row" id="mwBkU">2nd p. sg.</th>
<td id="mwBkY"><i id="mwBkc">you</i></td><td id="mwBkg"><i id="mwBkk">you</i></td><td id="mwBko"><i id="mwBks">your</i></td><td id="mwBkw"><i id="mwBk0">yours</i></td><td id="mwBk4"><i id="mwBk8">yourself</i></td></tr>
<tr id="mwBlA">
