<tr id="mwBGI">
<td id="mwBGM">World Almanac (1999)</td><td id="mwBGQ">145,000,000</td><td id="mwBGU">8<span typeof="mw:Entity" id="mwBGY"> </span><span typeof="mw:Entity" id="mwBGc"> </span><span typeof="mw:Entity" id="mwBGg"> </span><span typeof="mw:Entity" id="mwBGk"> </span><span typeof="mw:Entity" id="mwBGo"> </span><span typeof="mw:Entity" id="mwBGs"> </span><span typeof="mw:Entity" id="mwBGw"> </span><span typeof="mw:Entity" id="mwBG0"> </span><span typeof="mw:Entity" id="mwBG4"> </span><span typeof="mw:Entity" id="mwBG8"> </span>(2005)</td><td id="mwBHA">275,000,000</td><td id="mwBHE">5</td></tr>
<tr id="mwBHI">
