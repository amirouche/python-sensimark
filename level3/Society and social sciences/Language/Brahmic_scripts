<tr style="font-size: 75%;" id="mwBo0">
<th id="mwBo4">ISO</th><th id="mwBo8">ka</th><th id="mwBpA">kha</th><th id="mwBpE">ga</th><th id="mwBpI">gha</th><th id="mwBpM">ṅ</th><th id="mwBpQ">ca</th><th id="mwBpU">cha</th><th id="mwBpY">ja</th><th id="mwBpc">jha</th><th id="mwBpg">ñ</th><th id="mwBpk">ṭa</th><th id="mwBpo">ṭha</th><th id="mwBps">ḍa</th><th id="mwBpw">ḍha</th><th id="mwBp0">ṇa</th><th id="mwBp4">ta</th><th id="mwBp8">tha</th><th id="mwBqA">da</th><th id="mwBqE">dha</th><th id="mwBqI">na</th><th id="mwBqM">ṉ</th><th id="mwBqQ">pa</th><th id="mwBqU">pha</th><th id="mwBqY">ba</th><th id="mwBqc">bha</th><th id="mwBqg">ma</th><th id="mwBqk">ya</th><th id="mwBqo">ra</th><th id="mwBqs">ṟ</th><th id="mwBqw">la</th><th id="mwBq0">ḷa</th><th id="mwBq4">ḻ</th><th id="mwBq8">v</th><th id="mwBrA">śa</th><th id="mwBrE">ṣa</th><th id="mwBrI">sa</th><th id="mwBrM">h</th></tr>
