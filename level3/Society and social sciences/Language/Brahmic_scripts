<td id="mwAVw">ક</td><td id="mwAV0">ખ</td><td id="mwAV4">ગ</td><td id="mwAV8">ઘ</td><td id="mwAWA">ઙ</td><td id="mwAWE">ચ</td><td id="mwAWI">છ</td><td id="mwAWM">જ</td><td id="mwAWQ">ઝ</td><td id="mwAWU">ઞ</td><td id="mwAWY">ટ</td><td id="mwAWc">ઠ</td><td id="mwAWg">ડ</td><td id="mwAWk">ઢ</td><td id="mwAWo">ણ</td><td id="mwAWs">ત</td><td id="mwAWw">થ</td><td id="mwAW0">દ</td><td id="mwAW4">ધ</td><td id="mwAW8">ન</td><td style="background:#ccc;" id="mwAXA"></td><td id="mwAXE">પ</td><td id="mwAXI">ફ</td><td id="mwAXM">બ</td><td id="mwAXQ">ભ</td><td id="mwAXU">મ</td><td id="mwAXY">ય</td><td id="mwAXc">ર</td><td style="background:#ccc;" id="mwAXg"></td><td id="mwAXk">લ</td><td id="mwAXo">ળ</td><td style="background:#ccc;" id="mwAXs"></td><td id="mwAXw">વ</td><td id="mwAX0">શ</td><td id="mwAX4">ષ</td><td id="mwAX8">સ</td><td id="mwAYA">હ</td></tr>
<tr id="mwAYE">
<th style="font-size: 75%;" id="mwAYI"><a rel="mw:WikiLink" href="./Odia_script" title="Odia script" id="mwAYM" class="mw-redirect">Odia</a></th>
<td id="mwAYQ">କ</td><td id="mwAYU">ଖ</td><td id="mwAYY">ଗ</td><td id="mwAYc">ଘ</td><td id="mwAYg">ଙ</td><td id="mwAYk">ଚ</td><td id="mwAYo">ଛ</td><td id="mwAYs">ଜ</td><td id="mwAYw">ଝ</td><td id="mwAY0">ଞ</td><td id="mwAY4">ଟ</td><td id="mwAY8">ଠ</td><td id="mwAZA">ଡ</td><td id="mwAZE">ଢ</td><td id="mwAZI">ଣ</td><td id="mwAZM">ତ</td><td id="mwAZQ">ଥ</td><td id="mwAZU">ଦ</td><td id="mwAZY">ଧ</td><td id="mwAZc">ନ</td><td style="background:#ccc;" id="mwAZg"></td><td id="mwAZk">ପ</td><td id="mwAZo">ଫ</td><td id="mwAZs">ବ</td><td id="mwAZw">ଭ</td><td id="mwAZ0">ମ</td><td id="mwAZ4">ୟ</td><td id="mwAZ8">ର</td><td style="background:#ccc;" id="mwAaA"></td><td id="mwAaE">ଲ</td><td id="mwAaI">ଳ</td><td style="background:#ccc;" id="mwAaM"></td><td id="mwAaQ">ୱ</td><td id="mwAaU">ଶ</td><td id="mwAaY">ଷ</td><td id="mwAac">ସ</td><td id="mwAag">ହ</td></tr>
<tr id="mwAak">
<th style="font-size: 75%;" id="mwAao"><a rel="mw:WikiLink" href="./Gurmukhī_script" title="Gurmukhī script" id="mwAas" class="mw-redirect">Gurmukhi</a></th>
<td id="mwAaw">ਕ</td><td id="mwAa0">ਖ</td><td id="mwAa4">ਗ</td><td id="mwAa8">ਘ</td><td id="mwAbA">ਙ</td><td id="mwAbE">ਚ</td><td id="mwAbI">ਛ</td><td id="mwAbM">ਜ</td><td id="mwAbQ">ਝ</td><td id="mwAbU">ਞ</td><td id="mwAbY">ਟ</td><td id="mwAbc">ਠ</td><td id="mwAbg">ਡ</td><td id="mwAbk">ਢ</td><td id="mwAbo">ਣ</td><td id="mwAbs">ਤ</td><td id="mwAbw">ਥ</td><td id="mwAb0">ਦ</td><td id="mwAb4">ਧ</td><td id="mwAb8">ਨ</td><td style="background:#ccc;" id="mwAcA"></td><td id="mwAcE">ਪ</td><td id="mwAcI">ਫ</td><td id="mwAcM">ਬ</td><td id="mwAcQ">ਭ</td><td id="mwAcU">ਮ</td><td id="mwAcY">ਯ</td><td id="mwAcc">ਰ</td><td style="background:#ccc;" id="mwAcg"></td><td id="mwAck">ਲ</td><td id="mwAco">ਲ਼</td><td style="background:#ccc;" id="mwAcs"></td><td id="mwAcw">ਵ</td><td id="mwAc0">ਸ਼</td><td style="background:#ccc;" id="mwAc4"></td><td id="mwAc8">ਸ</td><td id="mwAdA">ਹ</td></tr>
