<td style="border-right: 0;" id="mwBFs"><span class="IPA" about="#mwt419" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"IPAlink","href":"./Template:IPAlink"},"params":{"1":{"wt":"θ"}},"i":0}}]}' id="mwBFw"><a rel="mw:WikiLink" href="./Voiceless_dental_fricative" title="Voiceless dental fricative">θ</a></span>*</td>
<td style="border-right: 0;" id="mwBF0"><span class="IPA" about="#mwt420" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"IPAlink","href":"./Template:IPAlink"},"params":{"1":{"wt":"s"}},"i":0}}]}' id="mwBF4"><a rel="mw:WikiLink" href="./Voiceless_alveolar_sibilant" title="Voiceless alveolar sibilant" class="mw-redirect">s</a></span></td><td style="border-left: 0;" id="mwBF8"></td>
<td style="border-right: 0;" id="mwBGA">(<span class="IPA" about="#mwt421" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"IPAlink","href":"./Template:IPAlink"},"params":{"1":{"wt":"ʃ"}},"i":0}}]}' id="mwBGE"><a rel="mw:WikiLink" href="./Voiceless_palato-alveolar_fricative" title="Voiceless palato-alveolar fricative" class="mw-redirect">ʃ</a></span>)</td>
<td style="border-right: 0;" id="mwBGI"><span class="IPA" about="#mwt422" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"IPAlink","href":"./Template:IPAlink"},"params":{"1":{"wt":"x"}},"i":0}}]}' id="mwBGM"><a rel="mw:WikiLink" href="./Voiceless_velar_fricative" title="Voiceless velar fricative">x</a></span></td></tr>
<tr id="mwBGQ">
<th id="mwBGU"><a rel="mw:WikiLink" href="./Lateral_consonant" title="Lateral consonant" id="mwBGY">Lateral</a></th>
<td colspan="2" id="mwBGc"></td>
<td colspan="2" id="mwBGg"></td>
<td style="border-right: 0;" id="mwBGk"></td><td style="border-left: 0;" id="mwBGo"><span class="IPA" about="#mwt423" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"IPAlink","href":"./Template:IPAlink"},"params":{"1":{"wt":"l"}},"i":0}}]}' id="mwBGs"><a rel="mw:WikiLink" href="./Alveolar_lateral_approximant" title="Alveolar lateral approximant" class="mw-redirect">l</a></span></td>
