13}},"\n* ",{"template":{"target":{"wt":"cite book\n","href":"./Template:Cite_book"},"params":{"last":{"wt":"Sherwood"},"first":{"wt":"Robert E."},"author-link":{"wt":"Robert E. Sherwood"},"year":{"wt":"1948"},"title":{"wt":"Roosevelt and Hopkins: An Intimate History"},"location":{"wt":"New York"},"publisher":{"wt":"Harper and Brothers"},"ref":{"wt":"harv"}},"i":14}},"\n* ",{"template":{"target":{"wt":"cite book\n","href":"./Template:Cite_book"},"params":{"year":{"wt":"2009"},"origyear":{"wt":"2007"},"editor1-last":{"wt":"Weiss"},"editor1-first":{"wt":"Thomas G."},"editor2-last":{"wt":"Daws"},"editor2-first":{"wt":"Sam"},"title":{"wt":"The Oxford Handbook on the United Nations"},"publisher":{"wt":"Oxford University Press"},"isbn":{"wt":"978-0-19-956010-3"},"ref":{"wt":"harv"}},"i":15}},"\n* ",{"template":{"target":{"wt":"cite book\n","href":"./Template:Cite_book"},"params":{"last":{"wt":"Wistrich"},"first":{"wt":"Robert S."},"author-link":{"wt":"Robert S. Wistrich"},"year":{"wt":"2010"},"title":{"wt":"A Lethal Obsession: Anti-Semitism from Antiquity to the Global Jihad"},"location":{"wt":"New York"},"publisher":{"wt":"Random House"},"isbn":{"wt":"978-1-4000-6097-9"},"ref":{"wt":"harv"}},"i":16}},"\n",{"template":{"target":{"wt":"refend","href":"./Template:Refend"},"params":{},"i":17}}]}' id="mwB64">.mw-parser-output .refbegin{font-size:90%;margin-bottom:0.5em}.mw-parser-output .refbegin-hanging-indents>ul{list-style-type:none;margin-left:0}.mw-parser-output .refbegin-hanging-indents>ul>li,.mw-parser-output .refbegin-hanging-indents>dl>dd{margin-left:0;padding-left:3.2em;text-indent:-3.2em;list-style:none}.mw-parser-output .refbegin-100{font-size:100%}</style><div class="refbegin   columns references-column-width" style="-moz-column-width: 35em; -webkit-column-width: 35em; column-width: 35em;" about="#mwt515" id="mwB68">
