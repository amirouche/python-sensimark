<p id="mwBg"><span typeof="mw:Nowiki mw:Transclusion" about="#mwt4" data-mw='{"parts":[{"template":{"target":{"wt":"EngvarB","href":"./Template:EngvarB"},"params":{"date":{"wt":"April 2017"}},"i":0}}]}' id="mwBw"></span><link rel="mw:PageProp/Category" href="./Category:EngvarB_from_April_2017" about="#mwt4" id="mwCA"/>
<span typeof="mw:Nowiki mw:Transclusion" about="#mwt5" data-mw='{"parts":[{"template":{"target":{"wt":"Use dmy dates","href":"./Template:Use_dmy_dates"},"params":{"date":{"wt":"February 2019"}},"i":0}}]}' id="mwCQ"></span><link rel="mw:PageProp/Category" href="./Category:Use_dmy_dates_from_February_2019" about="#mwt5" id="mwCg"/></p>
<table class="infobox vcard" style="width:22em" about="#mwt11" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"Infobox organization\n","href":"./Template:Infobox_organization"},"params":{"image":{"wt":"International Monetary Fund logo.svg"},"caption":{"wt":"Coat of arms"},"map":{"wt":""},"motto":{"wt":""},"formation":{"wt":"{{Start date and age|1945|12|27|df=yes}}"},"extinction":{"wt":""},"type":{"wt":"[[International financial institutions|International financial institution]]"},"status":{"wt":""},"purpose":{"wt":"Promote international monetary co-operation, facilitate [[international trade]], foster sustainable economic growth, make resources available to members experiencing [[balance of payments]] difficulties&lt;ref name=\"about\"/>"},"headquarters":{"wt":"[[Washington, D.C.]] [[U.S.]]"},"coords":{"wt":"{{Coord|38|53|56|N|77|2|39|W|display=inline,title}}"},"language":{"wt":"English&lt;ref name = \"Boughton 2001 7n5\">{{Harvnb|Boughton|2001|p=[https://books.google.com/books?id=dY8Wm4-8yGYC&amp;pg=PA7 7 n.5]}}.&lt;/ref>"},"leader_title":{"wt":"Managing Director (MD)"},"leader_name":{"wt":"[[Christine Lagarde]]"},"main_organ":{"wt":"Board of Governors"},"parent_organization":{"wt":"[[United Nations]]&lt;ref>{{cite web|url=https://www.imf.org/external/np/exr/facts/imfwb.htm|title=Factsheet: The IMF and the World Bank|date=21 September 2015|accessdate=1 December 2015|publisher=IMF}}&lt;/ref>&lt;ref>{{cite web|title=About the IMF Overview|url=https://www.imf.org/external/about/overview.htm|website=www.imf.org|accessdate=1 August 2017}}&lt;/ref>"},"affiliations":{"wt":""},"budget":{"wt":""},"remarks":{"wt":""},"image_size":{"wt":"150px"},"name":{"wt":"International Monetary Fund"},"image_border":{"wt":""},"size":{"wt":"200px"},"msize":{"wt":""},"mcaption":{"wt":""},"abbreviation":{"wt":"IMF"},"location":{"wt":"Headquartered in [[Washington, D.C.]]"},"region":{"wt":"Worldwide"},"membership":{"wt":"189 countries"},"num_staff":{"wt":"2,400&lt;ref