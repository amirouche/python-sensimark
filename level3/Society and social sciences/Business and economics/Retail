<tr id="mwBGw">
<td id="mwBG0"><a rel="mw:WikiLink" href="./Poland" title="Poland" id="mwBG4">Poland</a></td><td id="mwBG8">83%</td></tr>
<tr id="mwBHA">
<td id="mwBHE"><a rel="mw:WikiLink" href="./Hungary" title="Hungary" id="mwBHI">Hungary</a></td><td id="mwBHM">87%</td></tr>
<tr id="mwBHQ">
