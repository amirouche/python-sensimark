a15":{"wt":"56"},"label16":{"wt":"(15) &apos;&apos;&apos;{{MEX}}&apos;&apos;&apos;"},"data16":{"wt":"51"},"label17":{"wt":"(16) &apos;&apos;&apos;{{ITA}}&apos;&apos;&apos;"},"data17":{"wt":"50"},"label18":{"wt":"(17) &apos;&apos;&apos;{{ESP}}&apos;&apos;&apos;"},"data18":{"wt":"43"},"label19":{"wt":"(18) &apos;&apos;&apos;{{BAN}}&apos;&apos;&apos;"},"data19":{"wt":"41"},"label20":{"wt":"(19) &apos;&apos;&apos;{{THA}}&apos;&apos;&apos;"},"data20":{"wt":"40"},"label21":{"wt":"(20) &apos;&apos;&apos;{{EGY}}&apos;&apos;&apos;"},"data21":{"wt":"40"},"caption":{"wt":"{{resize|88%|The twenty largest countries by agricultural output (in nominal terms) at peak level as of 2018, according to the [[IMF]] and [[CIA World Factbook]].}}"},"isbn":{"wt":"9780309042482"},"date":{"wt":"1990-02-01"}},"i":0}}]}' id="mwAUQ">
