<li><a rel="mw:WikiLink" href="./Woodall_number" title="Woodall number">Woodall (<span class="texhtml texhtml-big" style="font-size:90%;"><i>n</i>·2<sup><i>n</i></sup><span typeof="mw:Entity"> </span>−<span typeof="mw:Entity"> </span>1</span>)</a></li>
<li><a rel="mw:WikiLink" href="./Cuban_prime" title="Cuban prime">Cuban (<span class="texhtml texhtml-big" style="font-size:90%;"><i>x</i><sup>3</sup><span typeof="mw:Entity"> </span>−<span typeof="mw:Entity"> </span><i>y</i><sup>3</sup>)/(<i>x</i><span typeof="mw:Entity"> </span>−<span typeof="mw:Entity"> </span><i>y</i></span>)</a></li>
