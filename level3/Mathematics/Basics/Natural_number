MC"},"accessdate":{"wt":""},"isbn":{"wt":"978-0-486-45792-5"}},"i":12}},"\n* ",{"template":{"target":{"wt":"Citation ","href":"./Template:Citation"},"params":{"last":{"wt":"Morash"},"first":{"wt":"Ronald P."},"year":{"wt":"1991"},"title":{"wt":"Bridge to Abstract Mathematics: Mathematical Proof and Structures"},"edition":{"wt":"Second"},"publisher":{"wt":"Mcgraw-Hill College"},"location":{"wt":""},"page":{"wt":""},"url":{"wt":"https://books.google.com/books?id=fH9YAAAAYAAJ"},"accessdate":{"wt":""},"isbn":{"wt":"978-0-07-043043-3"}},"i":13}},"\n* ",{"template":{"target":{"wt":"Citation ","href":"./Template:Citation"},"params":{"last":{"wt":"Musser"},"first":{"wt":"Gary L."},"last2":{"wt":"Peterson"},"first2":{"wt":"Blake E."},"last3":{"wt":"Burger"},"first3":{"wt":"William F."},"year":{"wt":"2013"},"title":{"wt":"Mathematics for Elementary Teachers: A Contemporary Approach"},"edition":{"wt":"10th"},"publisher":{"wt":"[[Wiley Global Education]]"},"location":{"wt":""},"page":{"wt":""},"isbn":{"wt":"978-1-118-45744-3"},"url":{"wt":"https://books.google.com/books?id=b3dbAgAAQBAJ"},"accessdate":{"wt":""}},"i":14}},"\n* ",{"template":{"target":{"wt":"Citation ","href":"./Template:Citation"},"params":{"last":{"wt":"Szczepanski"},"first":{"wt":"Amy F."},"last2":{"wt":"Kositsky"},"first2":{"wt":"Andrew P."},"year":{"wt":"2008"},"title":{"wt":"The Complete Idiot&apos;s Guide to Pre-algebra"},"publisher":{"wt":"Penguin Group"},"location":{"wt":""},"page":{"wt":""},"url":{"wt":"https://books.google.com/books?id=wLA2tlR_LYYC"},"accessdate":{"wt":""},"isbn":{"wt":"978-1-59257-772-9"}},"i":15}},"\n* ",{"template":{"target":{"wt":"Citation ","href":"./Template:Citation"},"params":{"last":{"wt":"Thomson"},"first":{"wt":"Brian S."},"last2":{"wt":"Bruckner"},"first2":{"wt":"Judith B."},"last3":{"wt":"Bruckner"},"first3":{"wt":"Andrew M."},"year":{"wt":"2008"},"title":{"wt":"Elementary Real Analysis"},"publisher":{"wt":"ClassicalRealAnalysis.com"},"edition":{"wt":"Second"},"location":{"