                <mrow class="MJX-TeXAtom-ORD">
                  <mfrac>
                    <mn>1</mn>
                    <mrow>
                      <mn>1</mn>
                      <mo>+</mo>
                      <mrow class="MJX-TeXAtom-ORD">
                        <mfrac>
                          <mn>1</mn>
                          <msup>
                            <mn>2</mn>
                            <mrow class="MJX-TeXAtom-ORD">
                              <mn>2</mn>
                            </mrow>
