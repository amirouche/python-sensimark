        <mo>,</mo>
        <mn>4</mn>
        <mi>π<!-- π --></mi>
        <mi>i</mi>
        <mo>,</mo>
        <mo>…<!-- … --></mo>
        <mo fence="false" stretchy="false">}</mo>
        <mo>=</mo>
        <mo fence="false" stretchy="false">{</mo>
        <mn>2</mn>
        <mi>π<!-- π --></mi>
        <mi>k</mi>
