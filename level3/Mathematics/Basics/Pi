                    <mi mathvariant="normal">∞<!-- ∞ --></mi>
                  </mrow>
                </munderover>
                <mrow>
                  <mo>(</mo>
                  <mrow>
                    <mn>1</mn>
                    <mo>−<!-- − --></mo>
                    <mrow class="MJX-TeXAtom-ORD">
                      <mfrac>
                        <mn>1</mn>
