          <mfrac>
            <mn>4</mn>
            <mrow>
              <mn>6</mn>
              <mo>×<!-- × --></mo>
              <mn>7</mn>
              <mo>×<!-- × --></mo>
              <mn>8</mn>
            </mrow>
          </mfrac>
        </mrow>
        <mo>−<!-- − --></mo>
        <mrow class="MJX-TeXAtom-ORD">
          <mfrac>
            <mn>4</mn>
