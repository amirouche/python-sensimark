                <mrow class="MJX-TeXAtom-ORD">
                  <mfrac>
                    <mn>9</mn>
                    <mn>12</mn>
                  </mfrac>
                </mrow>
                <mo>+</mo>
                <mrow class="MJX-TeXAtom-ORD">
                  <mfrac>
                    <mn>10</mn>
                    <mn>12</mn>
                  </mfrac>
                </mrow>
              </mtd>
              <mtd>
                <mo>=</mo>
                <mrow class="MJX-TeXAtom-ORD">
                  <mfrac>
