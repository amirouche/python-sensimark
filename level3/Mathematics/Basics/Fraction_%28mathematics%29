        <mo>,</mo>
        <mi>b</mi>
        <mi>d</mi>
        <mo stretchy="false">)</mo>
      </mstyle>
    </mrow>
    <annotation encoding="application/x-tex">{\displaystyle (a,b)\cdot (c,d)=(ac,bd)}</annotation>
  </semantics>
