        <mrow class="MJX-TeXAtom-ORD">
          <mstyle displaystyle="false" scriptlevel="0">
            <mfrac>
              <mn>51</mn>
              <mn>4</mn>
            </mfrac>
          </mstyle>
        </mrow>
        <mo>⋅<!-- ⋅ --></mo>
        <mrow class="MJX-TeXAtom-ORD">
          <mstyle displaystyle="false" scriptlevel="0">
            <mfrac>
              <mn>1</mn>
              <mn>26</mn>
            </mfrac>
          </mstyle>
        </mrow>
        <mo>=</mo>
