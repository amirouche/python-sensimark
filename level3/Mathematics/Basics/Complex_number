              <mrow>
                <mi>z</mi>
                <mo>−<!-- − --></mo>
                <mrow class="MJX-TeXAtom-ORD">
                  <mover>
                    <mi>z</mi>
                    <mo accent="false">¯<!-- ¯ --></mo>
                  </mover>
                </mrow>
              </mrow>
              <mrow>
                <mn>2</mn>
