        <mi mathvariant="normal">∠<!-- ∠ --></mi>
        <mi>φ<!-- φ --></mi>
        <mo>.</mo>
      </mstyle>
    </mrow>
    <annotation encoding="application/x-tex">{\displaystyle z=r\angle \varphi .}</annotation>
