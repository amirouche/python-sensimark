        <mi>b</mi>
        <mo stretchy="false">)</mo>
        <mo>+</mo>
        <mi>sin</mi>
        <mo>⁡<!-- ⁡ --></mo>
        <mo stretchy="false">(</mo>
        <mi>a</mi>
        <mo stretchy="false">)</mo>
        <mi>cos</mi>
