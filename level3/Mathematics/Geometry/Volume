            <mn>4</mn>
            <mn>3</mn>
          </mfrac>
        </mrow>
        <mi>π<!-- π --></mi>
        <msup>
          <mi>r</mi>
          <mrow class="MJX-TeXAtom-ORD">
            <mn>3</mn>
          </mrow>
        </msup>
        <mo>=</mo>
        <mrow>
