                </mrow>
              </mrow>
              <mrow class="MJX-TeXAtom-ORD">
                <mn>2</mn>
              </mrow>
            </msup>
            <mo>+</mo>
            <msup>
              <mrow class="MJX-TeXAtom-ORD">
                <mrow>
                  <mo>|</mo>
