        <mrow class="MJX-TeXAtom-ORD">
          <mfrac>
            <mn>1</mn>
            <mn>4</mn>
          </mfrac>
        </mrow>
        <mrow class="MJX-TeXAtom-ORD">
          <msqrt>
            <mo stretchy="false">(</mo>
            <mi>a</mi>
            <mo>+</mo>
            <mi>b</mi>
            <mo>−<!-- − --></mo>
            <mi>c</mi>
            <mo stretchy="false">)</mo>
            <mo stretchy="false">(</mo>
            <mi>a</mi>
            <mo>−<!-- − --></mo>
            <mi>b</mi>
            <mo>+</mo>
            <mi>c</mi>
            <mo stretchy="false">)</mo>
            <mo stretchy="false">(</mo>
