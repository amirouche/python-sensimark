            <mi>a</mi>
            <mo>+</mo>
            <mi>b</mi>
            <mo>+</mo>
            <mi>c</mi>
            <mo stretchy="false">)</mo>
            <mo stretchy="false">(</mo>
            <mi>a</mi>
            <mo>+</mo>
            <mi>b</mi>
