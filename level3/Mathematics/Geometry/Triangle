        <mi>r</mi>
        <mo>=</mo>
        <mrow class="MJX-TeXAtom-ORD">
          <mfrac>
            <mrow>
              <mi>a</mi>
              <mi>b</mi>
              <mi>c</mi>
            </mrow>
