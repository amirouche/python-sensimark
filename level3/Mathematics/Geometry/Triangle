                        <mi>a</mi>
                        <mo stretchy="false">¯<!-- ¯ --></mo>
                      </mover>
                    </mrow>
                  </mrow>
                </mtd>
                <mtd>
                  <mn>1</mn>
                </mtd>
              </mtr>
              <mtr>
                <mtd>
                  <mi>b</mi>
                </mtd>
                <mtd>
                  <mrow class="MJX-TeXAtom-ORD">
