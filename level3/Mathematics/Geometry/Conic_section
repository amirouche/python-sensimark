                <mtd>
                  <mi>z</mi>
                </mtd>
              </mtr>
            </mtable>
          </mrow>
          <mo>)</mo>
        </mrow>
        <mrow>
          <mo>(</mo>
          <mrow class="MJX-TeXAtom-ORD">
            <mtable rowspacing="4pt" columnspacing="1em">
              <mtr>
                <mtd>
                  <mi>A</mi>
                </mtd>
                <mtd>
                  <mi>B</mi>
                  <mrow class="MJX-TeXAtom-ORD">
                    <mo>/</mo>
                  </mrow>
                  <mn>2</mn>
                </mtd>
                <mtd>
                  <mi>D</mi>
                  <mrow class="MJX-TeXAtom-ORD">
                    <mo>/</mo>
                  </mrow>
                  <mn>2</mn>
                </mtd>
              </mtr>
              <mtr>
                <mtd>
                  <mi>B</mi>
                  <mrow class="MJX-TeXAtom-ORD">
