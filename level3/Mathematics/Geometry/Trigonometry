        <mi>csc</mi>
        <mo>⁡<!-- ⁡ --></mo>
        <mi>A</mi>
        <mo>=</mo>
        <mrow class="MJX-TeXAtom-ORD">
          <mfrac>
            <mn>1</mn>
            <mrow>
              <mi>sin</mi>
              <mo>⁡<!-- ⁡ --></mo>
              <mi>A</mi>
            </mrow>
          </mfrac>
        </mrow>
        <mo>=</mo>
        <mrow class="MJX-TeXAtom-ORD">
