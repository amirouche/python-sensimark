<tr id="mwhA">
<td style="background:#f2f2f2; text-align:center;" id="mwhQ"><b id="mwhg"><a rel="mw:WikiLink" href="./Turn_(geometry)" title="Turn (geometry)" id="mwhw">Turns</a></b><span typeof="mw:Entity" id="mwiA"> </span><span typeof="mw:Entity" id="mwiQ"> </span></td>
<td style="width:3em;" id="mwig">0</td>
<td style="width:3em;" id="mwiw">(0,<span typeof="mw:Entity" id="mwjA"> </span> <span class="sfrac nowrap" style="display:inline-block; vertical-align:-0.5em; font-size:85%; text-align:center;" about="#mwt21" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"sfrac","href":"./Template:Sfrac"},"params":{"1":{"wt":"1"},"2":{"wt":"4"}},"i":0}}]}' id="mwjQ"><span style="display:block; line-height:1em; margin:0 0.1em;">1</span><span class="visualhide">/</span><span style="display:block; line-height:1em; margin:0 0.1em; border-top:1px solid;">4</span></span>)</td>
<td style="width:3em;" id="mwjg"><span class="sfrac nowrap" style="display:inline-block; vertical-align:-0.5em; font-size:85%; text-align:center;" about="#mwt22" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"sfrac","href":"./Template:Sfrac"},"params":{"1":{"wt":"1"},"2":{"wt":"4"}},"i":0}}]}' id="mwjw"><span style="display:block; line-height:1em; margin:0 0.1em;">1</span><span class="visualhide">/</span><span style="display:block; line-height:1em; margin:0 0.1em; border-top:1px solid;">4</span></span></td>
<td style="width:3em;" id="mwkA">(<span class="sfrac nowrap" style="display:inline-block; vertical-align:-0.5em; font-size:85%; text-align:center;" about="#mwt23" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"sfrac","href":"./Template:Sfrac"},"params":{"1":{"wt":"1"},"2":{"wt":"4"}},"i":0}}]}' id="mwkQ"><span style="display:block; line-height:1em; margin:0 0.1em;">1</span><span class="visualhide">/</span><span style="display:block; line-height:1em; margin:0 0.1em; border-top:1px solid;">4</span></span>,<span typeof="mw:Entity" id="mwkg"> </span> <span class="sfrac nowrap" style="display:inline-block; vertical-align:-0.5em; font-size:85%; text-align:center;" about="#mwt24" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"sfrac","href":"./Template:Sfrac"},"params":{"1":{"wt":"1"},"2":{"wt":"2"}},"i":0}}]}' id="mwkw"><span style="display:block; line-height:1em; margin:0 0.1em;">1</span><span class="visualhide">/</span><span style="display:block; line-height:1em; margin:0 0.1em; border-top:1px solid;">2</span></span>)</td>
<td style="width:3em;" id="mwlA"><span class="sfrac nowrap" style="display:inline-block; vertical-align:-0.5em; font-size:85%; text-align:center;" about="#mwt25" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"sfrac","href":"./Template:Sfrac"},"params":{"1":{"wt":"1"},"2":{"wt":"2"}},"i":0}}]}' id="mwlQ"><span style="display:block; line-height:1em; margin:0 0.1em;">1</span><span class="visualhide">/</span><span style="display:block; line-height:1em; margin:0 0.1em; border-top:1px solid;">2</span></span></td>
<td style="width:3em;" id="mwlg">(<span class="sfrac nowrap" style="display:inline-block; vertical-align:-0.5em; font-size:85%; text-align:center;" about="#mwt26" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"sfrac","href":"./Template:Sfrac"},"params":{"1":{"wt":"1"},"2":{"wt":"2"}},"i":0}}]}' id="mwlw"><span style="display:block; line-height:1em; margin:0 0.1em;">1</span><span class="visualhide">/</span><span style="display:block; line-height:1em; margin:0 0.1em; border-top:1px solid;">2</span></span>,<span typeof="mw:Entity" id="mwmA"> </span> 1)</td>
<td style="width:3em;" id="mwmQ">1</td></tr>
<tr id="mwmg">
<td style="background:#f2f2f2; text-align:center;" id="mwmw"><b id="mwnA"><a rel="mw:WikiLink" href="./Radian" title="Radian" id="mwnQ">Radians</a></b></td>
<td id="mwng">0</td>
<td id="mwnw">(0, <span class="sfrac nowrap" style="display:inline-block; vertical-align:-0.5em; font-size:85%; text-align:center;" about="#mwt27" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"sfrac","href":"./Template:Sfrac"},"params":{"1":{"wt":"1"},"2":{"wt":"2"}},"i":0}}]}' id="mwoA"><span style="display:block; line-height:1em; margin:0 0.1em;">1</span><span class="visualhide">/</span><span style="display:block; line-height:1em; margin:0 0.1em; border-top:1px solid;">2</span></span><i id="mwoQ"><span class="texhtml " about="#mwt28" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"pi","href":"./Template:Pi"},"params":{},"i":0}}]}' id="mwog">π</span></i>)</td>
