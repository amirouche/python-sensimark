            </mrow>
            <mo fence="false" stretchy="false">⟩<!-- ⟩ --></mo>
          </mrow>
          <mo>)</mo>
        </mrow>
        <mo>=</mo>
        <mi>cos</mi>
        <mo>⁡<!-- ⁡ --></mo>
        <mo stretchy="false">(</mo>
        <mi>θ<!-- θ --></mi>
        <mo stretchy="false">)</mo>
        <mtext> </mtext>
        <mrow>
          <mo symmetric="true">‖</mo>
          <mrow class="MJX-TeXAtom-ORD">
