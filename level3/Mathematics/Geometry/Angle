<td id="mwpw">(<span class="sfrac nowrap" style="display:inline-block; vertical-align:-0.5em; font-size:85%; text-align:center;" about="#mwt31" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"sfrac","href":"./Template:Sfrac"},"params":{"1":{"wt":"1"},"2":{"wt":"2"}},"i":0}}]}' id="mwqA"><span style="display:block; line-height:1em; margin:0 0.1em;">1</span><span class="visualhide">/</span><span style="display:block; line-height:1em; margin:0 0.1em; border-top:1px solid;">2</span></span><i id="mwqQ"><span class="texhtml " about="#mwt32" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"pi","href":"./Template:Pi"},"params":{},"i":0}}]}' id="mwqg">π</span></i>, <i id="mwqw"><span class="texhtml " about="#mwt33" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"pi","href":"./Template:Pi"},"params":{},"i":0}}]}' id="mwrA">π</span></i>)</td>
<td id="mwrQ"><i id="mwrg"><span class="texhtml " about="#mwt34" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"pi","href":"./Template:Pi"},"params":{},"i":0}}]}' id="mwrw">π</span></i></td>
