          <mi>r</mi>
          <mo>=</mo>
          <mrow class="MJX-TeXAtom-ORD">
            <msqrt>
              <msup>
                <mi>x</mi>
                <mrow class="MJX-TeXAtom-ORD">
                  <mn>2</mn>
