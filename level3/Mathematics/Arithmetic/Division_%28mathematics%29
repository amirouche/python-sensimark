              <mi>s</mi>
            </mrow>
            <mrow>
              <msup>
                <mi>r</mi>
                <mrow class="MJX-TeXAtom-ORD">
                  <mn>2</mn>
                </mrow>
              </msup>
              <mo>+</mo>
              <msup>
