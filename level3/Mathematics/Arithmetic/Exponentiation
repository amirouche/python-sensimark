        <mn>1</mn>
        <msup>
          <mo stretchy="false">)</mo>
          <mrow class="MJX-TeXAtom-ORD">
            <mfrac>
              <mn>1</mn>
              <mn>2</mn>
            </mfrac>
          </mrow>
        </msup>
        <mo>=</mo>
