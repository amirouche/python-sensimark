              <mtd>
                <mrow class="MJX-TeXAtom-ORD">
                  <mtext>(two-sided identity)</mtext>
                </mrow>
              </mtd>
            </mtr>
            <mtr>
              <mtd>
                <msup>
                  <mi>x</mi>
                  <mrow class="MJX-TeXAtom-ORD">
                    <mn>0</mn>
                  </mrow>
                </msup>
