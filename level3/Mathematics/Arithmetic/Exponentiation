                          </mrow>
                        </msup>
                        <mo>×<!-- × --></mo>
                        <mo>⋯<!-- ⋯ --></mo>
                        <mo>×<!-- × --></mo>
                        <msup>
                          <mi>b</mi>
                          <mrow class="MJX-TeXAtom-ORD">
                            <mfrac>
                              <mn>1</mn>
                              <mi>n</mi>
                            </mfrac>
                          </mrow>
                        </msup>
                      </mrow>
                      <mo>⏟<!-- ⏟ --></mo>
                    </munder>
                  </mrow>
                  <mrow class="MJX-TeXAtom-ORD">
                    <mi>n</mi>
                    <mspace width="thinmathspace"></mspace>
                    <mrow class="MJX-TeXAtom-ORD">
                      <mrow class="MJX-TeXAtom-ORD">
                        <mtext>times</mtext>
                      </mrow>
                    </mrow>
                  </mrow>
                </munder>
              </mtd>
            </mtr>
            <mtr>
              <mtd></mtd>
              <mtd>
                <mi></mi>
                <mo>=</mo>
                <msup>
                  <mi>b</mi>
                  <mrow class="MJX-TeXAtom-ORD">
                    <munder>
                      <mrow class="MJX-TeXAtom-OP MJX-fixedlimits">
