              </mtd>
              <mtd>
                <mi></mi>
                <mo>=</mo>
                <mi>f</mi>
                <mo stretchy="false">(</mo>
                <mi>x</mi>
                <mo stretchy="false">)</mo>
                <mo>⋅<!-- ⋅ --></mo>
                <mi>g</mi>
                <mo stretchy="false">(</mo>
                <mi>x</mi>
                <mo stretchy="false">)</mo>
              </mtd>
