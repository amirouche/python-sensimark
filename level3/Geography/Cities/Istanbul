<table class="infobox vcard" style="width:22em" about="#mwt640" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"Infobox political party\n","href":"./Template:Infobox_political_party"},"params":{"name":{"wt":"Members of Parliament for İstanbul&lt;br>{{small|[[Turkish general election, November 2015]]}}&lt;br>[[File:İstanbul2015Kasım.png|220px]]"},"colorcode":{"wt":"#000099"},"seats1_title":{"wt":"[[Justice and Development Party (Turkey)|AK Party]]"},"seats1":{"wt":"{{Composition bar|46|88|hex=#fdc400}}"},"seats2_title":{"wt":"[[Republican People&apos;s Party (Turkey)|CHP]]"},"seats2":{"wt":"{{Composition bar|28|88|hex=#cc0000}}"},"seats3_title":{"wt":"[[Peoples&apos; Democratic Party (Turkey)|HDP]]"},"seats3":{"wt":"{{Composition bar|7|88|hex=#91007B}}"},"seats4_title":{"wt":"[[Nationalist Movement Party|MHP]]"},"seats4":{"wt":"{{Composition bar|7|88|hex=#870000}}"}},"i":0}}]}' id="mwAzE"><tbody><tr><th colspan="2" style="text-align:center;font-size:125%;font-weight:bold;padding-top:0; padding-bottom:0;"><div style="padding-top:0.3em; padding-bottom:0.3em; border-top:2px solid #000099; border-bottom:2px solid #000099; line-height: 1;"><div style="display:inline;" class="fn org">Members of Parliament for İstanbul<br/><span style="font-size:85%;"><a rel="mw:WikiLink" href="./Turkish_general_election,_November_2015" title="Turkish general election, November 2015" class="mw-redirect">Turkish general election, November 2015</a></span><br/><figure-inline typeof="mw:Image"><a href="./File:İstanbul2015Kasım.png"><img resource="./File:İstanbul2015Kasım.png" src="//upload.wikimedia.org/wikipedia/commons/thumb/7/7c/%C4%B0stanbul2015Kas%C4%B1m.png/220px-%C4%B0stanbul2015Kas%C4%B1m.png" data-file-width="789" data-file-height="417" data-file-type="bitmap" height="116" width="220" srcset="//upload.wikimedia.org/wikipedia/commons/thumb/7/7c/%C4%B0stanbul2015Kas%C4%B1m.png/440px-%C4%B0stanbul2015Kas%C4%B1m.png 2x, //upload.wikimedia.org/wikipedia/commons/thumb/7/7c/%C4%B0stanbul2015Kas%C4%B1m.png/330px-%C4%B0stanbul2015Kas%C4%B1m.png 1.5x"/></a></figure-inline></div></div></th></tr><tr><th scope="row" style="line-height:1.3em;"><a rel="mw:WikiLink" href="./Justice_and_Development_Party_(Turkey)" title="Justice and Development Party (Turkey)">AK Party</a></th><td style="line-height:1.3em;"><div style="width: 100px;"><span class="nowrap">46 / 88</span><div role="img" style="box-sizing: border-box; border: 1px solid #aaa;  height: 1.15em; position: relative;"><div style="background-color: 
