<td style="background:#FF8D00;color:#000000;" id="mwAbs">22.8<br id="mwAbw"/>(73.0)</td>
<td style="background:#FFE388;color:#000000;" id="mwAb0">18.4<br id="mwAb4"/>(65.1)</td>
<td style="background:#CFCFFF;color:#000000;" id="mwAb8">13.8<br id="mwAcA"/>(56.8)</td>
