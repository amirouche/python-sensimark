<td style="text-align:right;" about="#mwt372">19,341</td><td id="mwAtk"><a rel="mw:WikiLink" href="./Tanzania" title="Tanzania" id="mwAto">Tanzania</a></td><td id="mwAts"><a rel="mw:WikiLink" href="./Lake_Assal_(Djibouti)" title="Lake Assal (Djibouti)" id="mwAtw">Lake Assal</a></td><td about="#mwt373" typeof="mw:Transclusion" style="text-align:right;" data-mw='{"parts":["|| ",{"template":{"target":{"wt":"convert","href":"./Template:Convert"},"params":{"1":{"wt":"-155"},"2":{"wt":"m"},"disp":{"wt":"table"}},"i":0}}," "]}' id="mwAt0">−155</td>
<td style="text-align:right;" about="#mwt373">−509</td><td id="mwAt4"><a rel="mw:WikiLink" href="./Djibouti" title="Djibouti" id="mwAt8">Djibouti</a></td></tr>
<tr id="mwAuA">
<td id="mwAuE">Europe</td><td id="mwAuI"><a rel="mw:WikiLink" href="./Mount_Elbrus" title="Mount Elbrus" id="mwAuM">Mount Elbrus</a></td><td about="#mwt374" typeof="mw:Transclusion" style="text-align:right;" data-mw='{"parts":["|| ",{"template":{"target":{"wt":"convert","href":"./Template:Convert"},"params":{"1":{"wt":"5642"},"2":{"wt":"m"},"disp":{"wt":"table"}},"i":0}}," "]}' id="mwAuQ">5,642</td>
<td style="text-align:right;" about="#mwt374">18,510</td><td id="mwAuU">Russia</td><td id="mwAuY"><a rel="mw:WikiLink" href="./Caspian_Sea" title="Caspian Sea" id="mwAuc">Caspian Sea</a></td><td about="#mwt375" typeof="mw:Transclusion" style="text-align:right;" data-mw='{"parts":["|| ",{"template":{"target":{"wt":"convert","href":"./Template:Convert"},"params":{"1":{"wt":"-28"},"2":{"wt":"m"},"disp":{"wt":"table"}},"i":0}}," "]}' id="mwAug">−28</td>
