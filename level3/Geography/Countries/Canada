AEfoC&amp;pg=PP1"},"year":{"wt":"2007"},"publisher":{"wt":"McClelland &amp; Stewart"},"isbn":{"wt":"978-0-7710-2181-7"}},"i":24}},"\n* ",{"template":{"target":{"wt":"cite book ","href":"./Template:Cite_book"},"params":{"first":{"wt":"Paul R"},"last":{"wt":"Magocsi"},"title":{"wt":"Encyclopedia of Canada&apos;s peoples"},"publisher":{"wt":"Society of Ontario, University of Toronto Press"},"year":{"wt":"1999"},"isbn":{"wt":"978-0-8020-2938-6"},"url":{"wt":"https://books.google.com/books?id=dbUuX0mnvQMC"}},"i":25}},"\n* ",{"template":{"target":{"wt":"cite book ","href":"./Template:Cite_book"},"params":{"first":{"wt":"Jonathan F."},"last":{"wt":"Vance"},"title":{"wt":"A History of Canadian Culture"},"url":{"wt":"https://books.google.com/books?id=TOR9SwAACAAJ"},"year":{"wt":"2011"},"publisher":{"wt":"Oxford University Press"},"isbn":{"wt":"978-0-19-544422-3"}},"i":26}},"\n&apos;&apos;&apos;Indigenous peoples&apos;&apos;&apos;\n* ",{"template":{"target":{"wt":"cite book ","href":"./Template:Cite_book"},"params":{"last":{"wt":"Dickason"},"first":{"wt":"Olive Patricia"},"last2":{"wt":"McNab"},"first2":{"wt":"David T."},"title":{"wt":"Canada&apos;s First Nations: A History of Founding Peoples from Earliest Times"},"edition":{"wt":"4th"},"publisher":{"wt":"Oxford University Press"},"location":{"wt":"Toronto, Ontario, Canada"},"year":{"wt":"2009"},"isbn":{"wt":"978-0-19-542892-6"},"url":{"wt":"https://books.google.com/books?id=EZ4MAQAAMAAJ"}},"i":27}},"\n* ",{"template":{"target":{"wt":"cite book ","href":"./Template:Cite_book"},"params":{"editor1-last":{"wt":"Ladner"},"editor1-first":{"wt":"Kiera L."},"editor2-first":{"wt":"Myra"},"editor2-last":{"wt":"Tait"},"title":{"wt":"Surviving Canada: Indigenous peoples celebrate 150 years of betrayal"},"year":{"wt":"2017"},"publisher":{"wt":"Arp Books"},"location":{"wt":"Winnipeg, Manitoba"},"isbn":{"wt":"978-1-894037-89-1"},"url":{"wt":"https://books.google.com/books?isbn=1894037898"}},"i":28}},"\n|}\n",{"template":{"target":{"wt":"r