."},"author-link":{"wt":"Upinder Singh"},"title":{"wt":"A History of Ancient and Medieval India: From the Stone Age to the 12th Century"},"year":{"wt":"2009"},"publisher":{"wt":"[[Longman]]"},"location":{"wt":"Delhi"},"isbn":{"wt":"978-81-317-1677-9"},"url":{"wt":"https://books.google.com/books?id=H3lUIIYxWkEC"}},"i":14}},"\n* ",{"template":{"target":{"wt":"citation","href":"./Template:Citation"},"params":{"last":{"wt":"Sripati"},"first":{"wt":"V."},"year":{"wt":"1998"},"title":{"wt":"Toward Fifty Years of Constitutionalism and Fundamental Rights in India: Looking Back to See Ahead (1950–2000)"},"journal":{"wt":"American University International Law Review"},"volume":{"wt":"14"},"issue":{"wt":"2"},"pages":{"wt":"413–496"}},"i":15}},"\n* ",{"template":{"target":{"wt":"citation","href":"./Template:Citation"},"params":{"last":{"wt":"Stein"},"first":{"wt":"B."},"author-link":{"wt":"Burton Stein"},"date":{"wt":"16 June 1998"},"title":{"wt":"A History of India"},"edition":{"wt":"1st"},"publisher":{"wt":"Wiley-Blackwell"},"place":{"wt":"Oxford"},"isbn":{"wt":"978-0-631-20546-3"},"url":{"wt":"https://books.google.com/books?id=SXdVS0SzQSAC"}},"i":16}},"\n* ",{"template":{"target":{"wt":"citation","href":"./Template:Citation"},"params":{"last":{"wt":"Stein"},"first":{"wt":"B."},"author-link":{"wt":"Burton Stein"},"editor-last":{"wt":"Arnold"},"editor-first":{"wt":"D."},"date":{"wt":"27 April 2010"},"title":{"wt":"A History of India"},"edition":{"wt":"2nd"},"publisher":{"wt":"Wiley-Blackwell"},"place":{"wt":"Oxford"},"isbn":{"wt":"978-1-4051-9509-6"},"url":{"wt":"https://books.google.com/books?id=QY4zdTDwMAQC"}},"i":17}},"\n* ",{"template":{"target":{"wt":"citation","href":"./Template:Citation"},"params":{"date":{"wt":"17 December 2009"},"title":{"wt":"Briefing Rooms: India"},"work":{"wt":"Economic Research Service"},"publisher":{"wt":"[[United States Department of Agriculture]]"},"url":{"wt":"http://www.ers.usda.gov/Briefing/India/"},"ref":{"wt":"{{sfnRef|United States Depart