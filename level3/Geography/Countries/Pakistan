<tr style="text-align:center;" id="mwBPI">
<td id="mwBPM">Gold</td><td id="mwBPQ">20.9 million ounces</td></tr>
<tr style="text-align:center;" id="mwBPU">
<td id="mwBPY">Coal</td><td id="mwBPc">175 billion tonnes</td></tr>
<tr style="text-align:center;" id="mwBPg">
<td id="mwBPk">Shale Gas</td><td id="mwBPo">105 trillion cubic feet</td></tr>
<tr style="text-align:center;" id="mwBPs">
<td id="mwBPw">Shale Oil</td><td id="mwBP0">9 billion barrels</td></tr>
