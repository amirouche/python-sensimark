<table class="infobox vcard" style="width:22em" about="#mwt6" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"Infobox body of water\n","href":"./Template:Infobox_body_of_water"},"params":{"name":{"wt":"Pacific Ocean"},"image":{"wt":"File:Pacific Ocean - en.png"},"alt":{"wt":"Map of the Pacific Ocean"},"coords":{"wt":"{{coord|0|N|160|W|region:XP_type:waterbody_dim:20000000|display=inline,title}}"},"area":{"wt":"{{convert|165250000|km2|abbr=on}}"},"depth":{"wt":"{{convert|4280|m|abbr=on}}"},"max-depth":{"wt":"{{convert|10911|m|abbr=on}}"},"volume":{"wt":"{{convert|710000000|km3|abbr=on}}"},"shore":{"wt":""},"islands":{"wt":"[[List of islands in the Pacific Ocean|List of islands]]"},"trenches":{"wt":""}},"i":0}}]}' id="mwCw"><tbody><tr><th colspan="2" class="fn org" style="text-align:center;font-size:125%;font-weight:bold;background-color: #cedeff; font-size: 125%;">Pacific Ocean</th></tr><tr><td colspan="2" style="text-align:center;line-height: 1.2; border-bottom: 1px solid #cedeff;"><figure-inline class="mw-default-size mw-image-border" typeof="mw:Image/Frameless" data-mw='{"caption":"Map of the Pacific Ocean"}'><a href="./File:Pacific_Ocean_-_en.png"><img alt="Map of the Pacific Ocean" resource="./File:Pacific_Ocean_-_en.png" src="//upload.wikimedia.org/wikipedia/commons/thumb/f/f6/Pacific_Ocean_-_en.png/250px-Pacific_Ocean_-_en.png" data-file-width="330" data-file-height="330" data-file-type="bitmap" height="250" width="250" srcset="//upload.wikimedia.org/wikipedia/commons/f/f6/Pacific_Ocean_-_en.png 2x, //upload.wikimedia.org/wikipedia/commons/f/f6/Pacific_Ocean_-_en.png 1.5x"/></a></figure-inline></td></tr><tr><th scope="row"><span title="Geographical coordinates">Coordinates</span></th><td><span class="plainlinks nourlexpansion"><a rel="mw:ExtLink" class="external text" href="//tools.wmflabs.org/geohack/geohack.php?pagename=Pacific_Ocean&amp;params=0_N_160_W_region:XP_type:waterbody_dim:20000000"><span class="geo-default"><span class="ge