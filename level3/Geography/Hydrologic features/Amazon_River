<section data-mw-section-id="30" id="mwAsY"><h3 id="List_by_length">List by length</h3>
<ol id="mwAsc"><li id="mwAsg"><span about="#mwt497" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"convert","href":"./Template:Convert"},"params":{"1":{"wt":"6400"},"2":{"wt":"km"},"3":{"wt":"mi"},"abbr":{"wt":"on"}},"i":0}}]}' id="mwAsk">6,400</span><span typeof="mw:Entity" about="#mwt497"> </span><span about="#mwt497">km (4,000</span><span typeof="mw:Entity" about="#mwt497"> </span><span about="#mwt497">mi)</span><sup about="#mwt501" class="mw-ref" id="cite_ref-britannica-amazon_2-7" rel="dc:references" typeof="mw:Extension/ref" data-mw='{"name":"ref","attrs":{"name":"britannica-amazon"}}'><a href="./Amazon_River#cite_note-britannica-amazon-2" style="counter-reset: mw-Ref 2;"><span class="mw-reflink-text">[2]</span></a></sup> [<span about="#mwt498" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"convert","href":"./Template:Convert"},"params":{"1":{"wt":"6275"},"2":{"wt":"km"},"3":{"wt":"mi"},"abbr":{"wt":"on"}},"i":0}}]}' id="mwAso">6,275</span><span typeof="mw:Entity" about="#mwt498"> </span><span about="#mwt498">km (3,899</span><span typeof="mw:Entity" about="#mwt498"> </span><span about="#mwt498">mi)</span> to <span about="#mwt499" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"convert","href":"./Template:Convert"},"params":{"1":{"wt":"7025"},"2":{"wt":"km"},"3":{"wt":"mi"},"abbr":{"wt":"on"}},"i":0}}]}' id="mwAss">7,025</span><span typeof="mw:Entity" about="#mwt499"> </span><span about="#mwt499">km (4,365</span><span typeof="mw:Entity" about="#mwt499"> </span><span about="#mwt499">mi)</span>]<sup about="#mwt503" class="mw-ref" id="cite_ref-liu-etal-river-length_3-7" rel="dc:references" typeof="mw:Extension/ref" data-mw='{"name":"ref","attrs":{"name":"liu-etal-river-length"}}'><a href="./Amazon_River#cite_note-liu-etal-river-length-3" style="counter-reset: mw-Ref 3;"><span class="mw-reflink-text">[3]</span></a></sup> – Amazon, South America</li>
<li id="mwAsw"><span about="#mwt506" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"Convert","href":"./Template:Convert"},"params":{"1":{"wt":"3250"},"2":{"wt":"km"},"3":{"wt":"mi"},"abbr":{"wt":"on"}},"i":0}}]}' id="mwAs0">3,250</span><span typeof="mw:Entity" about="#mwt506"> </span><span about="#mwt506">km (2,020</span><span typeof="mw:Entity" about="#mwt506"> </span><span about="#mwt506">mi)</span> – <a rel="mw:WikiLink" href="./Madeira_River" title="Madeira River" id="mwAs4">Madeira</a>, Bolivia/Brazil<sup about="#mwt649" class="mw-ref" id="cite_ref-81" rel="dc:references" typeof="mw:Extension/ref" data-mw='{"name":"ref","attrs":{},"body":{"id":"mw-reference-text-cite_note-81"}}'><a href="./Amazon_River#cite_note-81" style="counter-reset: mw-Ref 79;"><span class="mw-reflink-text">[79]</span></a></sup></li>
<li id="mwAs8"><span about="#mwt513" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"Convert","href":"./Template:Convert"},"params":{"1":{"wt":"3211"},"2":{"wt":"km"},"3":{"wt":"mi"},"abbr":{"wt":"on"}},"i":0}}]}' id="mwAtA">3,211</span><span typeof="mw:Entity" about="#mwt513"> </span><span about="#mwt513">km (1,995</span><span typeof="mw:Entity" about="#mwt513"> </span><span about="#mwt513">mi)</span> – <a rel="mw:WikiLink" href="./Purús_River" title="Purús River" id="mwAtE" class="mw-redirect">Purús</a>, Peru/Brazil<sup about="#mwt651" class="mw-ref" id="cite_ref-82" rel="dc:references" typeof="mw:Extension/ref" data-mw='{"name":"ref","attrs":{},"body":{"id":"mw-reference-text-cite_note-82"}}'><a href="./Amazon_River#cite_note-82" style="counter-reset: mw-Ref 80;"><span class="mw-reflink-text">[80]</span></a></sup></li>
<li id="mwAtI"><span about="#mwt516" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"Convert","href":"./Template:Convert"},"params":{"1":{"wt":"2820"},"2":{"wt":"km"},"3":{"wt":"mi"},"abbr":{"wt":"on"}},"i":0}}]}' id="mwAtM">2,820</span><span typeof="mw:Entity" about="#mwt516"> </span><span about="#mwt516">km (1,750</span><span typeof="mw:Entity" about="#mwt516"> </span><span about="#mwt516">mi)</span> – <a rel="mw:WikiLink" href="./Japurá_River" title="Japurá River" id="mwAtQ">Japurá or Caquetá</a>, Colombia/Brazil<sup about="#mwt653" class="mw-ref" id="cite_ref-83" rel="dc:references" typeof="mw:Extension/ref" data-mw='{"name":"ref","attrs":{},"body":{"id":"mw-reference-text-cite_note-83"}}'><a href="./Amazon_River#cite_note-83" style="counter-reset: mw-Ref 81;"><span class="mw-reflink-text">[81]</span></a></sup></li>
<li id="mwAtU"><span about="#mwt519" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"Convert","href":"./Template:Convert"},"params":{"1":{"wt":"2639"},"2":{"wt":"km"},"3":{"wt":"mi"},"abbr":{"wt":"on"}},"i":0}}]}' id="mwAtY">2,639</span><span typeof="mw:Entity" about="#mwt519"> </span><span about="#mwt519">km (1,640</span><span typeof="mw:Entity" about="#mwt519"> </span><span about="#mwt519">mi)</span> – <a rel="mw:WikiLink" href="./Tocantins_River" title="Tocantins River" id="mwAtc">Tocantins</a>, Brazil<sup about="#mwt655" class="mw-ref" id="cite_ref-84" rel="dc:references" typeof="mw:Extension/ref" data-mw='{"name":"ref","attrs":{},"body":{"id":"mw-reference-text-cite_note-84"}}'><a href="./Amazon_River#cite_note-84" style="counter-reset: mw-Ref 82;"><span class="mw-reflink-text">[82]</span></a></sup></li>
