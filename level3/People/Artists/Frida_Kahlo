ttenmann"},"first":{"wt":"Andrea"},"title":{"wt":"Kahlo"},"year":{"wt":"2003"},"publisher":{"wt":"[[Taschen]]"},"isbn":{"wt":"978-3-8228-5983-4"},"location":{"wt":""},"pages":{"wt":""},"ref":{"wt":"harv"}},"i":26}},"\n* ",{"template":{"target":{"wt":"cite book","href":"./Template:Cite_book"},"params":{"last":{"wt":"Lindauer"},"first":{"wt":"Margaret A."},"title":{"wt":"Devouring Frida: The Art History and Popular Celebrity of Frida Kahlo"},"year":{"wt":"1999"},"publisher":{"wt":"[[University Press of New England]]"},"isbn":{"wt":""},"location":{"wt":""},"pages":{"wt":""}},"i":27}},"\n* ",{"template":{"target":{"wt":"cite journal","href":"./Template:Cite_journal"},"params":{"last":{"wt":"Mahon"},"first":{"wt":"Alyce"},"title":{"wt":"The Lost Secret: Frida Kahlo and The Surrealist Imaginary"},"journal":{"wt":"[[Journal of Surrealism and the Americas]]"},"volume":{"wt":"5"},"issue":{"wt":"1–2"},"date":{"wt":"2011"},"issn":{"wt":""},"url":{"wt":""},"accessdate":{"wt":""},"pages":{"wt":"33–54"},"doi":{"wt":""},"ref":{"wt":"harv"},"via":{"wt":""}},"i":28}},"\n* ",{"template":{"target":{"wt":"cite book","href":"./Template:Cite_book"},"params":{"last1":{"wt":"Marnham"},"first1":{"wt":"Patrick"},"title":{"wt":"Dreaming with His Eyes Open: A Life of Diego Rivera"},"year":{"wt":"1998"},"publisher":{"wt":"[[University of California Press]]"},"isbn":{"wt":"978-0-520-22408-7"},"location":{"wt":""},"pages":{"wt":""},"ref":{"wt":"harv"}},"i":29}},"\n* ",{"template":{"target":{"wt":"cite journal","href":"./Template:Cite_journal"},"params":{"last1":{"wt":"Pankl"},"first1":{"wt":"Lis"},"last2":{"wt":"Blake"},"first2":{"wt":"Kevin"},"title":{"wt":"Made in Her Image: Frida Kahlo as Material Culture"},"journal":{"wt":"[[Material Culture]]"},"volume":{"wt":"44"},"issue":{"wt":"2"},"date":{"wt":"2012"},"issn":{"wt":""},"url":{"wt":""},"accessdate":{"wt":""},"via":{"wt":""},"pages":{"wt":"1–20"},"doi":{"wt":""}},"i":30}},"\n* ",{"template":{"target":{"wt":"cite book","href":"./Template:Cite