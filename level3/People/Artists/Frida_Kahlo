\""},"journal":{"wt":"[[Woman&apos;s Art Journal]]"},"volume":{"wt":"19"},"issue":{"wt":"2"},"date":{"wt":"1998–1999"},"issn":{"wt":""},"url":{"wt":""},"accessdate":{"wt":""},"via":{"wt":""},"pages":{"wt":"8–12"},"doi":{"wt":"10.2307/1358399"},"jstor":{"wt":"1358399"}},"i":13}},"\n* ",{"template":{"target":{"wt":"cite journal","href":"./Template:Cite_journal"},"params":{"last":{"wt":"Budrys"},"first":{"wt":"Valmantas"},"title":{"wt":"Neurological Deficits in the Life and Work of Frida Kahlo"},"journal":{"wt":"[[European Neurology]]"},"volume":{"wt":"55"},"issue":{"wt":"1"},"date":{"wt":"February 2006"},"issn":{"wt":"0014-3022"},"url":{"wt":"http://content.karger.com/produktedb/produkte.asp?typ=fulltext&amp;file=ENE2006055001004"},"accessdate":{"wt":"2008-01-22"},"pmid":{"wt":"16432301"},"pages":{"wt":"4–10"},"doi":{"wt":"10.1159/000091136"},"ref":{"wt":"harv"},"via":{"wt":""}},"i":14}},"\n* ",{"template":{"target":{"wt":"cite journal","href":"./Template:Cite_journal"},"params":{"last":{"wt":"Burns"},"first":{"wt":"Janet M.C."},"title":{"wt":"Looking as Women: The Paintings of Suzanne Valadon, Paula Modersohn-Becker and Frida Kahlo"},"journal":{"wt":"Atlantis"},"volume":{"wt":"18"},"issue":{"wt":"1–2"},"pages":{"wt":"25–46"},"via":{"wt":""},"date":{"wt":"February 2006"},"url":{"wt":"http://journals.msvu.ca/index.php/atlantis/article/view/5167"}},"i":15}},"\n* ",{"template":{"target":{"wt":"cite book","href":"./Template:Cite_book"},"params":{"last":{"wt":"Burrus"},"first":{"wt":"Christina"},"editor-last":{"wt":"Dexter"},"editor-first":{"wt":"Emma"},"location":{"wt":""},"pages":{"wt":""},"chapter":{"wt":"The Life of Frida Kahlo"},"title":{"wt":"Frida Kahlo"},"year":{"wt":"2005"},"publisher":{"wt":"[[Tate Modern]]"},"isbn":{"wt":"1-85437-586-5"},"ref":{"wt":"harv"}},"i":16}},"\n* ",{"template":{"target":{"wt":"cite book","href":"./Template:Cite_book"},"params":{"last1":{"wt":"Burrus"},"first1":{"wt":"Christina"},"author-mask":{"wt":""},"title":{"wt":"Frida Kahlo: &apos;