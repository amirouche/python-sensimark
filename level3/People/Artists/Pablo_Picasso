 Charney | first = Noah | author-link = | title = Pablo Picasso, art thief: the \\\"affaire des statuettes\\\" and its role in the foundation of modernist painting | journal = Arte, Individuo y Sociedad | volume = 26 (2) | pages = 187–197 | date = 23 January 2014 | url = http://revistas.ucm.es/index.php/ARIS/article/viewFile/39942/42607 }}&amp;lt;/ref>\"}&apos; data-mw=&apos;{\"name\":\"ref\",\"attrs\":{\"name\":\"thief\"},\"body\":{\"id\":\"mw-reference-text-cite_note-thief-34\"}}&apos;>&lt;a href=\"./Pablo_Picasso#cite_note-thief-34\" style=\"counter-reset: mw-Ref 34;\" data-parsoid=\"{}\">&lt;span class=\"mw-reflink-text\" data-parsoid=\"{}\">[34]&lt;/span>&lt;/a>&lt;/sup>\n&lt;sup about=\"#mwt1166\" class=\"mw-ref\" rel=\"dc:references\" typeof=\"mw:Extension/ref\" data-parsoid=&apos;{\"src\":\"&amp;lt;ref name=\\\"TIME - 08Apr2009 - Art&amp;apos;s Great Whodunit: The Mona Lisa Theft of 1911\\\">{{cite news|url=http://www.time.com/time/arts/article/0,8599,1894006,00.html|title=Art&amp;apos;s Great Whodunit: The Mona Lisa Theft of 1911|last=[[Richard Lacayo]]|date=7 April 2009|work=[[Time (magazine)|TIME]]|publisher=[[Time Inc.]]|accessdate=28 June 2013}}&amp;lt;/ref>\"}&apos; data-mw=&apos;{\"name\":\"ref\",\"attrs\":{\"name\":\"TIME - 08Apr2009 - Art&amp;apos;s Great Whodunit: The Mona Lisa Theft of 1911\"},\"body\":{\"id\":\"mw-reference-text-cite_note-TIME_-_08Apr2009_-_Art&amp;apos;s_Great_Whodunit:_The_Mona_Lisa_Theft_of_1911-35\"}}&apos;>&lt;a href=\"./Pablo_Picasso#cite_note-TIME_-_08Apr2009_-_Art&apos;s_Great_Whodunit:_The_Mona_Lisa_Theft_of_1911-35\" style=\"counter-reset: mw-Ref 35;\" data-parsoid=\"{}\">&lt;span class=\"mw-reflink-text\" data-parsoid=\"{}\">[35]&lt;/span>&lt;/a>&lt;/sup>\n&lt;sup about=\"#mwt1200\" class=\"mw-ref\" rel=\"dc:references\" typeof=\"mw:Extension/ref\" data-parsoid=&apos;{\"src\":\"&amp;lt;ref name=\\\"Richardson\\\">[https://books.google.com/books?id=AxcsQXDHd7YC&amp;amp;pg=PA78&amp;amp;dq=%22crystal+cubism%22,+A+Life+of+