<p id="mwCQ"><span typeof="mw:Nowiki mw:Transclusion" about="#mwt5" data-mw='{"parts":[{"template":{"target":{"wt":"featured article","href":"./Template:Featured_article"},"params":{},"i":0}}]}' id="mwCg"></span><link typeof="mw:Extension/indicator" about="#mwt5" data-mw='{"name":"indicator","attrs":{"name":"featured-star"},"body":{"extsrc":"[[File:cscr-featured.svg|20x20px\n |link=Wikipedia:Featured articles\n |\n |This is a featured article. Click here for more information.]]\n"}}'/><link rel="mw:PageProp/Category" href="./Category:Featured_articles" about="#mwt5" id="mwCw"/>
