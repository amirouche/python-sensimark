                    <mo stretchy="false">(</mo>
                    <mi>φ<!-- φ --></mi>
                    <mo stretchy="false">)</mo>
                  </mrow>
                  <mo>)</mo>
                </mrow>
                <mo>+</mo>
                <mrow class="MJX-TeXAtom-ORD">
                  <mfrac>
                    <mn>1</mn>
                    <mi>a</mi>
                  </mfrac>
                </mrow>
                <mrow class="MJX-TeXAtom-ORD">
                  <mfrac>
