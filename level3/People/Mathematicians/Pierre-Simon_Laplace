                      <mi mathvariant="normal">∂<!-- ∂ --></mi>
                      <mi>φ<!-- φ --></mi>
                    </mrow>
                  </mfrac>
                </mrow>
                <mrow>
                  <mo>(</mo>
                  <mrow>
