                      <mi>u</mi>
                    </mrow>
                    <mrow>
                      <mi mathvariant="normal">∂<!-- ∂ --></mi>
                      <mi>t</mi>
                    </mrow>
                  </mfrac>
                </mrow>
              </mtd>
              <mtd>
                <mi></mi>
                <mo>−<!-- − --></mo>
