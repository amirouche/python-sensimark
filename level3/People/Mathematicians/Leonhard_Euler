                  <mrow class="MJX-TeXAtom-ORD">
                    <mn>2</mn>
                  </mrow>
                </msup>
              </mfrac>
            </mrow>
            <mo>+</mo>
            <mo>⋯<!-- ⋯ --></mo>
            <mo>+</mo>
            <mrow class="MJX-TeXAtom-ORD">
              <mfrac>
