<table class="infobox biography vcard" style="width:22em" about="#mwt7" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"Infobox person\n","href":"./Template:Infobox_person"},"params":{"name":{"wt":"Walt Disney"},"image":{"wt":"Walt Disney 1946.JPG"},"caption":{"wt":"Disney in 1946"},"birth_name":{"wt":"Walter Elias Disney"},"birth_date":{"wt":"{{Birth date|1901|12|5}}"},"birth_place":{"wt":"Chicago, Illinois, U.S."},"death_date":{"wt":"{{Death date and age|1966|12|15|1901|12|5}}"},"death_place":{"wt":"[[Burbank, California]], U.S."},"occupation":{"wt":"{{Flatlist|\n* Entrepreneur\n* animator\n* voice actor\n* film producer\n}}"},"boards":{"wt":"[[The Walt Disney Company]] (1923–1966)"},"relations":{"wt":"See [[Disney family]]"},"awards":{"wt":"{{Plainlist|\n* 26 [[Academy Awards]]&lt;br />(22 competitive, 4 honorary)\n* 3 [[Golden Globe Award]]s\n* 1 [[Emmy Award]]\n}}"},"signature":{"wt":"Walt Disney 1942 signature.svg"}},"i":0}}]}' id="mwDQ"><tbody><tr><th colspan="2" style="text-align:center;font-size:125%;font-weight:bold"><div class="fn" style="display:inline">Walt Disney</div></th></tr><tr><td colspan="2" style="text-align:center"><figure-inline class="mw-default-size" typeof="mw:Image/Frameless"><a href="./File:Walt_Disney_1946.JPG"><img resource="./File:Walt_Disney_1946.JPG" src="//upload.wikimedia.org/wikipedia/commons/thumb/d/df/Walt_Disney_1946.JPG/220px-Walt_Disney_1946.JPG" data-file-width="450" data-file-height="675" data-file-type="bitmap" height="330" width="220" srcset="//upload.wikimedia.org/wikipedia/commons/thumb/d/df/Walt_Disney_1946.JPG/440px-Walt_Disney_1946.JPG 2x, //upload.wikimedia.org/wikipedia/commons/thumb/d/df/Walt_Disney_1946.JPG/330px-Walt_Disney_1946.JPG 1.5x"/></a></figure-inline><div>Disney in 1946</div></td></tr><tr><th scope="row">Born</th><td><div style="display:inline" class="nickname">Walter Elias Disney</div><br/><span style="display:none">(<span class="bday">1901-12-05</span>)</span>December 5, 1901<