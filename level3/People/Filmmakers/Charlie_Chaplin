<table class="infobox biography vcard" style="width:22em" about="#mwt6" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"Infobox person\n ","href":"./Template:Infobox_person"},"params":{"honorific_prefix":{"wt":"[[Sir]]"},"name":{"wt":"Charles Chaplin"},"honorific_suffix":{"wt":"{{post-nominals|size=100%|country=GBR|KBE}}"},"image":{"wt":"Charlie Chaplin portrait.jpg"},"caption":{"wt":"Publicity portrait, {{circa|1920}}"},"birth_name":{"wt":"Charles Spencer Chaplin"},"birth_date":{"wt":"{{birth date|1889|4|16|df=y}}"},"birth_place":{"wt":"[[England]]"},"death_date":{"wt":"{{death date and age|1977|12|25|1889|4|16|df=y}}"},"death_place":{"wt":"[[Manoir de Ban]], [[Riviera-Pays-d&apos;Enhaut District]], [[Vaud]], [[Switzerland]]"},"resting_place":{"wt":"Cimetière de Corsier-sur-Vevey, [[Corsier-sur-Vevey]], [[Riviera-Pays-d&apos;Enhaut District]], [[Vaud]], [[Switzerland]]"},"resting_place_coordinates":{"wt":"&lt;!-- {{coord|LAT|LONG|type:landmark|display=inline}} -->"},"monuments":{"wt":""},"residence":{"wt":""},"nationality":{"wt":""},"other_names":{"wt":"Charlie Chaplin"},"citizenship":{"wt":""},"education":{"wt":""},"alma_mater":{"wt":""},"occupation":{"wt":"{{hlist|Actor|director|composer|screenwriter|producer|editor}}"},"years_active":{"wt":"1899–1976"},"era":{"wt":""},"employer":{"wt":""},"organization":{"wt":""},"spouse":{"wt":"{{Unbulleted list |\n   | {{marriage|[[Mildred Harris]]|1918|1920|reason=divorced}}\n   | {{marriage|[[Lita Grey]]|1924|1927|reason=divorced}}\n   | {{marriage|[[Paulette Goddard]]|1936|1942|reason=divorced}}\n   | {{marriage|[[Oona O&apos;Neill]]|1943|&lt;!--Year omitted when marriage ends by death of article subject, per Template:Marriage instructions-->}}\n  }}"},"children":{"wt":""},"parents":{"wt":""},"relatives":{"wt":"[[Chaplin family]]"},"awards":{"wt":""},"signature":{"wt":"Firma de Charles Chaplin.svg"},"website":{"wt":"{{url|charliechaplin.com}}"}},"i":0}}]}' id="mwDA"><tbody><tr><th colspan="2" style=