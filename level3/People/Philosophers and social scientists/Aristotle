ponus"},"ref":{"wt":"harv"}},"i":138}},"\n* ",{"template":{"target":{"wt":"cite SEP ","href":"./Template:Cite_SEP"},"params":{"title":{"wt":"Aristotle&apos;s Influence"},"date":{"wt":"2018"},"url-id":{"wt":"aristotle-influence"},"edition":{"wt":"Spring 2018"},"ref":{"wt":"CITEREFAristotle&apos;s Influence2018"}},"i":139}},"\n",{"template":{"target":{"wt":"refend","href":"./Template:Refend"},"params":{},"i":140}}]}' id="mwBpA">.mw-parser-output .refbegin{font-size:90%;margin-bottom:0.5em}.mw-parser-output .refbegin-hanging-indents>ul{list-style-type:none;margin-left:0}.mw-parser-output .refbegin-hanging-indents>ul>li,.mw-parser-output .refbegin-hanging-indents>dl>dd{margin-left:0;padding-left:3.2em;text-indent:-3.2em;list-style:none}.mw-parser-output .refbegin-100{font-size:100%}</style><div class="refbegin   columns references-column-width" style="-moz-column-width: 36em; -webkit-column-width: 36em; column-width: 36em;" about="#mwt1024" id="mwBpE">
