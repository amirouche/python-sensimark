<div class="div-col columns column-width" style="-moz-column-width: 30em; -webkit-column-width: 30em; column-width: 30em;   " about="#mwt674" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"cmn","href":"./Template:Cmn"},"params":{"1":{"wt":"\n* [[Harold F. Cherniss]], major Plato scholar\n* [[Ellen Francis Mason]], translator of Plato\n"},"colwidth":{"wt":"30em"}},"i":0}}]}' id="mwBBY">
