-2"},"ref":{"wt":"harv"}},"i":26}},"\n\n:",{"template":{"target":{"wt":"Cite book ","href":"./Template:Cite_book"},"params":{"last":{"wt":"Meredith"},"first":{"wt":"Martin"},"authorlink":{"wt":"Martin Meredith"},"title":{"wt":"Mandela: A Biography"},"publisher":{"wt":"PublicAffairs"},"location":{"wt":"New York"},"year":{"wt":"2010"},"isbn":{"wt":"978-1-58648-832-1"},"ref":{"wt":"harv"}},"i":27}},"\n\n:",{"template":{"target":{"wt":"cite contribution ","href":"./Template:Cite_contribution"},"params":{"contribution":{"wt":"Democracy and Governance in Transition"},"last1":{"wt":"Muthien"},"first1":{"wt":"Yvonne"},"last2":{"wt":"Khosa"},"first2":{"wt":"Meshack"},"last3":{"wt":"Magubane"},"first3":{"wt":"Bernard"},"year":{"wt":"2000"},"title":{"wt":"Democracy and Governance Review: Mandela&apos;s Legacy 1994–1999"},"editor":{"wt":"Yvonne Muthien"},"editor2":{"wt":"Meshack Khosa"},"editor3":{"wt":"Bernard Magubane (eds.)"},"publisher":{"wt":"Human Sciences Research Council Press"},"location":{"wt":"Pretoria"},"pages":{"wt":"361–374"},"isbn":{"wt":"978-0-7969-1970-0"},"ref":{"wt":"harv"}},"i":28}},"\n\n:",{"template":{"target":{"wt":"cite journal ","href":"./Template:Cite_journal"},"params":{"last":{"wt":"Ndlovu-Gatsheni"},"first":{"wt":"Sabelo J."},"year":{"wt":"2014"},"title":{"wt":"From a &apos;Terrorist&apos; to Global Icon: A Critical Decolonial Ethical Tribute to Nelson Rolihlahla Mandela of South Africa"},"journal":{"wt":"Third World Quarterly"},"volume":{"wt":"35"},"number":{"wt":"6"},"pages":{"wt":"905–921"},"doi":{"wt":"10.1080/01436597.2014.907703"},"ref":{"wt":"harv"}},"i":29}},"\n\n: ",{"template":{"target":{"wt":"cite journal ","href":"./Template:Cite_journal"},"params":{"last":{"wt":"Oppenheim"},"first":{"wt":"Claire E."},"title":{"wt":"Nelson Mandela and the Power of Ubuntu"},"journal":{"wt":"Religions"},"year":{"wt":"2012"},"volume":{"wt":"3"},"number":{"wt":"2"},"pages":{"wt":"369–388"},"doi":{"wt":"10.3390/rel3020369"},"ref":{"wt":"harv"}},"i":30}},"\n\n