<tr><td> </td></tr><tr><td> </td><td rowspan="2" colspan="4"> </td></tr><tr><td> </td><td rowspan="3"> </td></tr><tr><td> </td><td rowspan="2" colspan="4"> </td></tr><tr><td> </td></tr><tr><td> </td><td rowspan="2" colspan="4"> </td><td rowspan="2" colspan="3"> </td></tr><tr><td> </td><td rowspan="7"> </td></tr><tr><td> </td><td rowspan="6" colspan="2"> </td><td rowspan="3"> </td><td rowspan="2" colspan="4"> </td></tr><tr><td> </td></tr><tr><td> </td><td rowspan="2" colspan="4"> </td></tr><tr><td> </td><td rowspan="3"> </td></tr><tr><td> </td><td rowspan="2" colspan="4"> </td></tr><tr><td> </td></tr><tr><td> </td><td rowspan="2" colspan="4" style="border:1px solid black;height:0.5em; padding:0 0.2em;background-color: #fb9;">3. <a rel="mw:WikiLink" href="./Olympias" title="Olympias">Olympias</a></td><td rowspan="2" colspan="11"> </td></tr><tr><td> </td><td> </td></tr><tr><td> </td><td> </td><td> </td><td> </td><td> </td><td> </td><td> </td><td> </td><td> </td><td> </td><td> </td><td> </td><td> </td><td> </td><td> </td><td> </td><td> </td></tr></tbody></table></td></tr></tbody></table></div>

