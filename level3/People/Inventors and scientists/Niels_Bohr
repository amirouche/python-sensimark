<table class="infobox biography vcard" style="width:22em" about="#mwt6" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"Infobox scientist\n ","href":"./Template:Infobox_scientist"},"params":{"honorific_suffix":{"wt":""},"name":{"wt":"Niels Bohr"},"image":{"wt":"Niels Bohr.jpg"},"alt":{"wt":"Photograph showing the head and shoulders of a man in a suit and tie"},"birth_name":{"wt":"Niels Henrik David Bohr"},"birth_date":{"wt":"{{dob|df=y|1885|10|7}}"},"death_date":{"wt":"{{nowr|{{dda|df=y|1962|11|18|1885|10|7}}}}"},"birth_place":{"wt":"[[Copenhagen]], Denmark"},"death_place":{"wt":"Copenhagen, Denmark"},"resting_place":{"wt":"[[Assistens Cemetery (Copenhagen)|Assistens Cemetery]]"},"alma_mater":{"wt":"[[University of Copenhagen]]"},"known_for":{"wt":"{{clist\n |framestyle    = border:none; padding:0;\n |liststyle     = list-style-type:disc; text-align:left;\n |title         = {{nobold|[[#Physics|Physics contributions]]}}\n   |[[Bohr magneton]]|[[Bohr model]]|[[Bohr radius]]|[[Bohr–Einstein debates]]|[[BKS theory|Bohr–Kramers–Slater theory]]\n   |[[Bohr–van Leeuwen theorem]]|[[Old quantum theory|Bohr–Sommerfeld theory]]|[[Complementarity (physics)|Complementarity]]\n   |[[Copenhagen interpretation]]}}"},"spouse":{"wt":"Margrethe Nørlund ({{abbr|m.|married}}&amp;nbsp;1912)"},"children":{"wt":"[[Aage Bohr|Aage]], [[Ernest Bohr|Ernest]], four other sons"},"awards":{"wt":"{{nowr|[[Nobel Prize in Physics]]&amp;nbsp;(1922)}}{{clist\n |framestyle    = border:none; padding:0;\n |liststyle     = list-style-type:disc; text-align:left;\n |title         = {{nobold|[[#Accolades|more accolades]]}}\n   |[[Hughes Medal]]&amp;nbsp;(1921)|[[Matteucci Medal]]&amp;nbsp;(1923)|[[Franklin Medal]]&amp;nbsp;(1926)\n   |[[Royal Society|Foreign Member of the Royal&amp;nbsp;Society]] (1926)|[[Max Planck Medal]]&amp;nbsp;(1930)\n   |[[Faraday Lectureship Prize]]&amp;nbsp;(1930)|[[Copley Medal]]&amp;nbsp;(1938)|[[Order of the&amp;nbsp;Elephant]] (1947)\n   |[[Atoms for Peace Award]]&amp;nbsp;(1957)|[[Sonning Prize]]&amp;nbsp;(1957)}}"},"field":{"wt":"[[Theoretical physics]]"},"workplaces":{"wt":"{{ubl\n |style         = text-indent:-1em; margin-left:1em;\n   |[[Trinity College, Cambridge|Trinity College]]|[[University of Copenhagen]]|[[Victoria University of Manchester]]}}"},"thesis_title":{"wt":"Studies on the Electron Theory of Metals"},"thesis_url":{"wt":"https://www.sciencedirect.com/science/article/pii/S187605030870015X"},"thesis_year":{"wt":"1911"},"doctoral_advisor":{"wt":"[[Christian Christiansen]]"},"academic_advisors":{"wt":"{{ubl|[[J. J. Thomson]]|[[Ernest Rutherford]]}}"},"doctoral_students":{"wt":"[[Hendrik Kramers]]"},"notable_students":{"wt":"[[Lev Landau]]"},"influences":{"wt":"{{ubl|[[Ernest Rutherford]]|[[Harald Høffding]]}}"},"influenced":{"wt":"{{ubl|[[Werner Heisenberg]]|[[Wolfgang Pauli]]|[[Paul Dirac]]|[[Lise Meitner]]|[[Max Delbrück]]}}"},"signature":{"wt":"Niels Bohr Signature.svg"}},"i":0}}]}' id="mwDA"><tbody><tr><th colspan="2" style="text-align:center;font-size:125%;font-weight:bold"><div class="fn" style="display:inline">Niels Bohr</div></th></tr><tr><td colspan="2" style="text-align:center"><figure-inline class="mw-default-size" typeof="mw:Image/Frameless" data-mw='{"caption":"Photograph showing the head and shoulders of a man in a suit and tie"}'><a href="./File:Niels_Bohr.jpg"><img alt="Photograph showing the head and shoulders of a man in a suit and tie" resource="./File:Niels_Bohr.jpg" src="//upload.wikimedia.org/wikipedia/commons/thumb/6/6d/Niels_Bohr.jpg/220px-Niels_Bohr.jpg" data-file-width="1456" data-file-height="2048" data-file-type="bitmap" height="309" width="220" srcset="//upload.wikimedia.org/wikipedia/commons/thumb/6/6d/Niels_Bohr.jpg/440px-Niels_Bohr.jpg 2x, //upload.wikimedia.org/wikipedia/commons/thumb/6/6d/Niels_Bohr.jpg/330px-Niels_Bohr.jpg 1.5x"/></a></figure-inline></td></tr><tr><th scope="row">Born</th><td><div style="display:inline" class="nickname">Niels Henrik David Bohr</div><