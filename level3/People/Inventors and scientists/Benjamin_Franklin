<span typeof="mw:Nowiki mw:Transclusion" about="#mwt5" data-mw='{"parts":[{"template":{"target":{"wt":"Use mdy dates","href":"./Template:Use_mdy_dates"},"params":{"date":{"wt":"May 2016"}},"i":0}}]}' id="mwCg"></span><link rel="mw:PageProp/Category" href="./Category:Use_mdy_dates_from_May_2016" about="#mwt5" id="mwCw"/></p>
<table class="infobox vcard" style="width:22em" about="#mwt6" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"Infobox officeholder\n","href":"./Template:Infobox_officeholder"},"params":{"honorific_prefix":{"wt":""},"name":{"wt":"Benjamin Franklin"},"image":{"wt":"Benjamin Franklin by Joseph Duplessis 1778.jpg"},"office":{"wt":"6th [[President of Pennsylvania]]"},"vicepresident":{"wt":"[[Charles Biddle]]&lt;br>[[Peter Muhlenberg]]&lt;br>[[David Redick]]"},"term_start":{"wt":"October 18, 1785"},"term_end":{"wt":"November 5, 1788"},"predecessor":{"wt":"[[John Dickinson (Pennsylvania and Delaware)|John Dickinson]]"},"successor":{"wt":"[[Thomas Mifflin]]"},"office1":{"wt":"[[United States Minister to Sweden]]"},"appointer1":{"wt":"[[Congress of the Confederation]]"},"term_start1":{"wt":"September 28, 1782"},"term_end1":{"wt":"April 3, 1783"},"predecessor1":{"wt":"Position established"},"successor1":{"wt":"[[Jonathan Russell]]"},"office2":{"wt":"[[United States Minister to France]]"},"appointer2":{"wt":"[[Continental Congress]]"},"term_start2":{"wt":"March 23, 1779"},"term_end2":{"wt":"May 17, 1785"},"predecessor2":{"wt":"Position established"},"successor2":{"wt":"[[Thomas Jefferson]]"},"office3":{"wt":"1st [[United States Postmaster General]]"},"term_start3":{"wt":"July 26, 1775"},"term_end3":{"wt":"November 7, 1776"},"predecessor3":{"wt":"Position established"},"successor3":{"wt":"[[Richard Bache]]"},"office4":{"wt":"[[List of Speakers of the Pennsylvania House of Representatives|Speaker of the Pennsylvania Assembly]]"},"term_start4":{"wt":"May 1764"},"term_end4":{"wt":"October 1764"},"predecessor4":{"wt":"[[Isaac Norris (statesman)|Isaac Norris]]"},"successor4":{"wt":"[[Isaac Norris (statesman)|Isaac Norris]]"},"birth_date":{"wt":"{{birth date|1706|1|17}}"},"birth_place":{"wt":"[[Boston]], [[Province of Massachusetts Bay|Massachusetts Bay]], [[British America]]"},"death_date":{"wt":"{{death date and age|1790|4|17|1706|1|17}}"},"death_place":{"w