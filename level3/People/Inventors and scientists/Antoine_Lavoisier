<table class="infobox biography vcard" style="width:22em" about="#mwt5" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"Infobox scientist\n ","href":"./Template:Infobox_scientist"},"params":{"name":{"wt":"Antoine-Laurent de Lavoisier"},"image":{"wt":"Antoine lavoisier color.jpg"},"caption":{"wt":"Line engraving by Louis Jean Desire Delaistre, after a design by Julien Leopold Boilly"},"birth_date":{"wt":"{{birth date|df=yes|1743|8|26}}"},"birth_place":{"wt":"[[Paris]], [[Kingdom of France|France]]"},"death_date":{"wt":"{{death date and age|df=yes|1794|5|8|1743|8|26}}"},"death_place":{"wt":"Paris, [[French First Republic|France]]"},"death_cause":{"wt":"Execution by [[guillotine]]"},"resting_place":{"wt":"[[Catacombs of Paris]]"},"alma_mater":{"wt":"[[Collège des Quatre-Nations]], [[University of Paris]]"},"influences":{"wt":"[[Guillaume-François Rouelle]], [[Étienne Condillac]]"},"notable_students":{"wt":"[[Éleuthère Irénée du Pont]]"},"known_for":{"wt":"{{unbulleted list\n                 | [[Combustion]] |Identified [[oxygen]] |Identified [[hydrogen]] | [[Stoichiometry]]\n                }}"},"footnotes":{"wt":""},"fields":{"wt":"Biologist, chemist"},"signature":{"wt":"Antoine Lavoisier Signature.svg"}},"i":0}}]}' id="mwCg"><tbody><tr><th colspan="2" style="text-align:center;font-size:125%;font-weight:bold"><div class="fn" style="display:inline">Antoine-Laurent de Lavoisier</div></th></tr><tr><td colspan="2" style="text-align:center"><figure-inline class="mw-default-size" typeof="mw:Image/Frameless"><a href="./File:Antoine_lavoisier_color.jpg"><img resource="./File:Antoine_lavoisier_color.jpg" src="//upload.wikimedia.org/wikipedia/commons/thumb/6/6c/Antoine_lavoisier_color.jpg/220px-Antoine_lavoisier_color.jpg" data-file-width="247" data-file-height="364" data-file-type="bitmap" height="324" width="220" srcset="//upload.wikimedia.org/wikipedia/commons/6/6c/Antoine_lavoisier_color.jpg 2x, //upload.wikimedia.org/wikipedia/commons/6/6c/Antoine_