<table class="infobox biography vcard" style="width:22em" about="#mwt5" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"Infobox person\n ","href":"./Template:Infobox_person"},"params":{"name":{"wt":"Roald Amundsen"},"image":{"wt":"Amundsen in fur skins.jpg"},"birth_name":{"wt":"Roald Engelbregt Gravning Amundsen"},"birth_date":{"wt":"{{birth date|df=y|1872|07|16}}&lt;ref>{{cite web|title=Østfold county, Borge in Borge, Parish register (official) nr. I 6 (1861–1874), Birth and baptism records 1872, page 114.|url=http://www.arkivverket.no/URN:NBN:no-a1450-kb20060404040371.jpg|accessdate=25 July 2012}}&lt;/ref>"},"birth_place":{"wt":"[[Borge, Østfold]], Norway"},"disappeared_date":{"wt":"{{disappeared date and age|df=y|1928|06|18|1872|07|16}}"},"disappeared_place":{"wt":"[[Barents Sea]]"},"occupation":{"wt":"Explorer"},"known_for":{"wt":"{{plainlist|style=text-indent:-1em; margin-left:1em;|\n* First to reach the [[South Pole]]\n* First to navigate the [[Northwest&amp;nbsp;Passage]]}}"},"awards":{"wt":"{{plainlist|\n* [[Hubbard Medal]] (1907)\n* [[Charles P. Daly Medal]] (1912)\n* [[Swedish Society for Anthropology and Geography|Vega Medal]] (1913)}}"},"nationality":{"wt":"Norwegian"},"parents":{"wt":"{{ubl|Jens Amundsen|Hanna Sahlqvist}}"},"signature":{"wt":"Roald Amundsen Signature.svg"}},"i":0}}]}' id="mwCA"><tbody><tr><th colspan="2" style="text-align:center;font-size:125%;font-weight:bold"><div class="fn" style="display:inline">Roald Amundsen</div></th></tr><tr><td colspan="2" style="text-align:center"><figure-inline class="mw-default-size" typeof="mw:Image/Frameless"><a href="./File:Amundsen_in_fur_skins.jpg"><img resource="./File:Amundsen_in_fur_skins.jpg" src="//upload.wikimedia.org/wikipedia/commons/thumb/6/6f/Amundsen_in_fur_skins.jpg/220px-Amundsen_in_fur_skins.jpg" data-file-width="2604" data-file-height="3503" data-file-type="bitmap" height="296" width="220" srcset="//upload.wikimedia.org/wikipedia/commons/thumb/6/6f/Amundsen_in_fur