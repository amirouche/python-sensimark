<table class="infobox biography vcard" style="width:22em" about="#mwt7" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"Infobox person\n ","href":"./Template:Infobox_person"},"params":{"name":{"wt":"Marco Polo"},"image":{"wt":"[[File:Marco Polo - costume tartare.jpg|250px]]"},"caption":{"wt":"Polo wearing a [[Tatar]] outfit, date of print unknown"},"birth_date":{"wt":"1254"},"birth_place":{"wt":"presumably Venice, [[Republic of Venice]]"},"death_date":{"wt":"{{death date and age|1324|1|8|1254|df=yes}}"},"death_place":{"wt":"[[Venice]], [[Republic of Venice]]"},"body_discovered":{"wt":""},"death_cause":{"wt":""},"resting_place":{"wt":"[[San Lorenzo, Venice|Church of San Lorenzo]]"},"resting_place_coordinates":{"wt":"{{Coord|45.4373|12.3455|type:landmark_region:IT|display=inline}}"},"residence":{"wt":""},"nationality":{"wt":"Italian"},"known_for":{"wt":"&apos;&apos;[[The Travels of Marco Polo]]&apos;&apos;"},"occupation":{"wt":"Merchant, explorer, writer"},"spouse":{"wt":"Donata Badoer"},"children":{"wt":"Fantina, Bellela and Moretta"},"parents":{"wt":"{{plainlist|\n*Mother: Nicole Anna Defuseh\n*Father: [[Niccolò Polo]]}}"}},"i":0}}]}' id="mwDQ"><tbody><tr><th colspan="2" style="text-align:center;font-size:125%;font-weight:bold"><div class="fn" style="display:inline">Marco Polo</div></th></tr><tr><td colspan="2" style="text-align:center"><figure-inline typeof="mw:Image"><a href="./File:Marco_Polo_-_costume_tartare.jpg"><img resource="./File:Marco_Polo_-_costume_tartare.jpg" src="//upload.wikimedia.org/wikipedia/commons/thumb/3/3d/Marco_Polo_-_costume_tartare.jpg/250px-Marco_Polo_-_costume_tartare.jpg" data-file-width="1095" data-file-height="1528" data-file-type="bitmap" height="349" width="250" srcset="//upload.wikimedia.org/wikipedia/commons/thumb/3/3d/Marco_Polo_-_costume_tartare.jpg/500px-Marco_Polo_-_costume_tartare.jpg 2x, //upload.wikimedia.org/wikipedia/commons/thumb/3/3d/Marco_Polo_-_costume_tartare.jpg/375px-Marco_Polo_-_costume_tar