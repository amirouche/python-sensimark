speare: An Oxford Guide"},"editor1-last":{"wt":"Wells"},"editor1-first":{"wt":"Stanley"},"editor1-link":{"wt":"Stanley Wells"},"editor2-last":{"wt":"Orlin"},"editor2-first":{"wt":"Lena Cowen"},"editor2-link":{"wt":"Lena Cowen Orlin"},"publisher":{"wt":"[[Oxford University Press]]"},"location":{"wt":"Oxford"},"year":{"wt":"2003"},"isbn":{"wt":"978-0-19-924522-2"},"ref":{"wt":"harv"}},"i":112}},"\n* ",{"template":{"target":{"wt":"cite book\n  ","href":"./Template:Cite_book"},"params":{"title":{"wt":"Shakespeare and Feminist Performance"},"last":{"wt":"Werner"},"first":{"wt":"Sarah"},"year":{"wt":"2001"},"location":{"wt":"London"},"publisher":{"wt":"[[Routledge]]"},"isbn":{"wt":"978-0-415-22729-2"},"ref":{"wt":"harv"}},"i":113}},"\n* ",{"template":{"target":{"wt":"cite web\n  ","href":"./Template:Cite_web"},"params":{"title":{"wt":"Visiting the Abbey"},"publisher":{"wt":"[[Westminster Abbey]]"},"url":{"wt":"http://www.westminster-abbey.org/archive/visit-us/highlights/poets-corner"},"accessdate":{"wt":"2 April 2016"},"dead-url":{"wt":"yes"},"archive-url":{"wt":"https://web.archive.org/web/20160403162702/http://www.westminster-abbey.org/archive/visit-us/highlights/poets-corner"},"archive-date":{"wt":"3 April 2016"},"ref":{"wt":"{{harvid|Westminster Abbey|n.d.}}"}},"i":114}},"\n* ",{"template":{"target":{"wt":"cite book\n  ","href":"./Template:Cite_book"},"params":{"title":{"wt":"Secret Shakespeare: Studies in Theatre, Religion and Resistance"},"last":{"wt":"Wilson"},"first":{"wt":"Richard"},"publisher":{"wt":"[[Manchester University Press]]"},"location":{"wt":"Manchester"},"year":{"wt":"2004"},"isbn":{"wt":"978-0-7190-7024-2"},"ref":{"wt":"harv"}},"i":115}},"\n* ",{"template":{"target":{"wt":"cite book\n  ","href":"./Template:Cite_book"},"params":{"title":{"wt":"The Plays of William Shakespeare with Notes of Various Commentators"},"editor-last":{"wt":"Wood"},"editor-first":{"wt":"Manley"},"year":{"wt":"1806"},"volume":{"wt":"I"},"location":{"wt":"London"},"publisher":{"w