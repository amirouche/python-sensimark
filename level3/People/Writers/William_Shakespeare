le"},"last":{"wt":"Shakespeare"},"first":{"wt":"William"},"author-link":{"wt":"William Shakespeare"},"display-authors":{"wt":"0"},"year":{"wt":"2007"},"editor1-last":{"wt":"Snyder"},"editor1-first":{"wt":"Susan"},"editor2-last":{"wt":"Curren-Aquino"},"editor2-first":{"wt":"Deborah"},"location":{"wt":"Cambridge"},"publisher":{"wt":"[[Cambridge University Press]]"},"isbn":{"wt":"978-0-521-22158-0"},"ref":{"wt":"{{harvid|Snyder|Curren-Aquino|2007}}"}},"i":100}},"\n* ",{"template":{"target":{"wt":"cite web\n  ","href":"./Template:Cite_web"},"params":{"title":{"wt":"Shakespeare Memorial"},"url":{"wt":"http://southwark.anglican.org/cathedral/tour/bill.htm"},"publisher":{"wt":"[[Southwark Cathedral]]"},"access-date":{"wt":"2 April 2016"},"dead-url":{"wt":"yes"},"archive-url":{"wt":"https://web.archive.org/web/20160304192458/http://www.southwark.anglican.org/cathedral/tour/bill.htm"},"archive-date":{"wt":"4 March 2016"},"ref":{"wt":"{{harvid|Southwark Cathedral|n.d.}}"}},"i":101}},"\n* ",{"template":{"target":{"wt":"cite book\n  ","href":"./Template:Cite_book"},"params":{"title":{"wt":"The Death of Tragedy"},"last":{"wt":"Steiner"},"first":{"wt":"George"},"author-link":{"wt":"George Steiner"},"publisher":{"wt":"[[Yale University Press]]"},"location":{"wt":"New Haven"},"year":{"wt":"1996"},"isbn":{"wt":"978-0-300-06916-7"},"ref":{"wt":"harv"}},"i":102}},"\n* ",{"template":{"target":{"wt":"cite book\n  ","href":"./Template:Cite_book"},"params":{"title":{"wt":"William Shakespeare: A Textual Companion"},"last":{"wt":"Taylor"},"first":{"wt":"Gary"},"author-link":{"wt":"Gary Taylor (scholar)"},"year":{"wt":"1987"},"location":{"wt":"Oxford"},"publisher":{"wt":"[[Oxford University Press]]"},"isbn":{"wt":"978-0-19-812914-1"},"ref":{"wt":"harv"}},"i":103}},"\n* ",{"template":{"target":{"wt":"cite book\n  ","href":"./Template:Cite_book"},"params":{"title":{"wt":"Reinventing Shakespeare: A Cultural History from the Restoration to the Present"},"last":{"wt":"Taylor"},"first":{"wt":"Gary