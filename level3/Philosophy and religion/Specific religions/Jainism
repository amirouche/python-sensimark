e"},"title":{"wt":"A Text-book of Indian History"},"url":{"wt":"{{Google books|xpABAAAAQAAJ|page=|keywords=|text=|plainurl=yes}}"},"year":{"wt":"1880"},"publisher":{"wt":"W.H. Allen &amp; Company"}},"i":109}},"\n* ",{"template":{"target":{"wt":"citation ","href":"./Template:Citation"},"params":{"first":{"wt":"Joan"},"last":{"wt":"Price"},"title":{"wt":"Sacred Scriptures of the World Religions: An Introduction"},"url":{"wt":"{{Google books|J0eycqQq9U4C|page=|keywords=|text=|plainurl=yes}}"},"year":{"wt":"2010"},"publisher":{"wt":"Bloomsbury Academic"},"isbn":{"wt":"978-0-8264-2354-2"}},"i":110}},"\n* ",{"template":{"target":{"wt":"citation ","href":"./Template:Citation"},"params":{"first":{"wt":"P.T."},"last":{"wt":"Raju"},"title":{"wt":"Structural Depths of Indian Thought"},"url":{"wt":"{{Google books|wZ_iahRQomwC|page=|keywords=|text=|plainurl=yes}}"},"year":{"wt":"1985"},"publisher":{"wt":"State University of New York Press"},"isbn":{"wt":"978-0-88706-139-4"}},"i":111}},"\n* ",{"template":{"target":{"wt":"citation ","href":"./Template:Citation"},"params":{"last":{"wt":"Rankin"},"first":{"wt":"Aidan D."},"last2":{"wt":"Mardia"},"first2":{"wt":"Kantilal"},"authorlink2":{"wt":"Kantilal Mardia"},"title":{"wt":"Living Jainism: An Ethical Science"},"url":{"wt":"{{Google books|bQxZAQAAQBAJ|page=|keywords=|text=|plainurl=yes}}"},"date":{"wt":"2013"},"publisher":{"wt":"John Hunt Publishing"},"isbn":{"wt":"978-1-78099-911-1"}},"i":112}},"\n* ",{"template":{"target":{"wt":"citation ","href":"./Template:Citation"},"params":{"first":{"wt":"Susan Verma"},"last":{"wt":"Mishra"},"first2":{"wt":"Himanshu Prabha"},"last2":{"wt":"Ray"},"title":{"wt":"The Archaeology of Sacred Spaces: The Temple in Western India, 2nd Century BCE–8th Century CE"},"url":{"wt":"{{Google books|CtDLDAAAQBAJ|page=|keywords=|text=|plainurl=yes}}"},"year":{"wt":"2016"},"publisher":{"wt":"Routledge"},"isbn":{"wt":"978-1-317-19374-6"}},"i":113}},"\n* ",{"template":{"target":{"wt":"citation ","href":"./Template