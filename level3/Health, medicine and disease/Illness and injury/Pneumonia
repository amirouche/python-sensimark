f65"},"2":{"wt":"less than 100"}},"i":2}},"\n",{"template":{"target":{"wt":"legend","href":"./Template:Legend"},"params":{"1":{"wt":"#fff200"},"2":{"wt":"100–700"}},"i":3}},"\n",{"template":{"target":{"wt":"legend","href":"./Template:Legend"},"params":{"1":{"wt":"#ffdc00"},"2":{"wt":"700–1,400"}},"i":4}},"\n",{"template":{"target":{"wt":"legend","href":"./Template:Legend"},"params":{"1":{"wt":"#ffc600"},"2":{"wt":"1,400–2,100"}},"i":5}},"\n",{"template":{"target":{"wt":"legend","href":"./Template:Legend"},"params":{"1":{"wt":"#ffb000"},"2":{"wt":"2,100–2,800"}},"i":6}},"\n",{"template":{"target":{"wt":"legend","href":"./Template:Legend"},"params":{"1":{"wt":"#ff9a00"},"2":{"wt":"2,800–3,500"}},"i":7}},"\n",{"template":{"target":{"wt":"legend","href":"./Template:Legend"},"params":{"1":{"wt":"#ff8400"},"2":{"wt":"3,500–4,200"}},"i":8}},"\n",{"template":{"target":{"wt":"legend","href":"./Template:Legend"},"params":{"1":{"wt":"#ff6e00"},"2":{"wt":"4,200–4,900"}},"i":9}},"\n",{"template":{"target":{"wt":"legend","href":"./Template:Legend"},"params":{"1":{"wt":"#ff5800"},"2":{"wt":"4,900–5,600"}},"i":10}},"\n",{"template":{"target":{"wt":"legend","href":"./Template:Legend"},"params":{"1":{"wt":"#ff4200"},"2":{"wt":"5,600–6,300"}},"i":11}},"\n",{"template":{"target":{"wt":"legend","href":"./Template:Legend"},"params":{"1":{"wt":"#ff2c00"},"2":{"wt":"6,300–7,000"}},"i":12}},"\n",{"template":{"target":{"wt":"legend","href":"./Template:Legend"},"params":{"1":{"wt":"#cb0000"},"2":{"wt":"more than 7,000"}},"i":13}},"\n",{"template":{"target":{"wt":"refend","href":"./Template:Refend"},"params":{},"i":14}}]}' id="mwArI"/><div class="refbegin   columns references-column-count references-column-count-2" style="-moz-column-count: 2; -webkit-column-count: 2; column-count: 2;" about="#mwt1177" id="mwArM">
<div class="legend" style="-webkit-column-break-inside: avoid;page-break-inside: avoid;break-inside: avoid-column"><span class="legend-color" style="display:inline-block; width:1.5em; height:1.5em; margin:1px 0; border:1px solid black; background-color:
