<div about="#mwt706" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"Col-begin","href":"./Template:Col-begin"},"params":{},"i":0}},"\n",{"template":{"target":{"wt":"Col-break","href":"./Template:Col-break"},"params":{},"i":1}},"\n",{"template":{"target":{"wt":"legend","href":"./Template:Legend"},"params":{"1":{"wt":"#b3b3b3"},"2":{"wt":"&lt;small>no data&lt;/small>"}},"i":2}},"\n",{"template":{"target":{"wt":"legend","href":"./Template:Legend"},"params":{"1":{"wt":"#ffff65"},"2":{"wt":"&lt;small>≤less 500&lt;/small>"}},"i":3}},"\n",{"template":{"target":{"wt":"legend","href":"./Template:Legend"},"params":{"1":{"wt":"#fff200"},"2":{"wt":"&lt;small>500–1000&lt;/small>"}},"i":4}},"\n",{"template":{"target":{"wt":"legend","href":"./Template:Legend"},"params":{"1":{"wt":"#ffdc00"},"2":{"wt":"&lt;small>1000–1500&lt;/small>"}},"i":5}},"\n",{"template":{"target":{"wt":"legend","href":"./Template:Legend"},"params":{"1":{"wt":"#ffc600"},"2":{"wt":"&lt;small>1500–2000&lt;/small>"}},"i":6}},"\n",{"template":{"target":{"wt":"legend","href":"./Template:Legend"},"params":{"1":{"wt":"#ffb000"},"2":{"wt":"&lt;small>2000–2500&lt;/small>"}},"i":7}},"\n",{"template":{"target":{"wt":"legend","href":"./Template:Legend"},"params":{"1":{"wt":"#ff9a00"},"2":{"wt":"&lt;small>2500–3000&lt;/small>"}},"i":8}},"\n",{"template":{"target":{"wt":"Col-break","href":"./Template:Col-break"},"params":{},"i":9}},"\n",{"template":{"target":{"wt":"legend","href":"./Template:Legend"},"params":{"1":{"wt":"#ff8400"},"2":{"wt":"&lt;small>3000–3500&lt;/small>"}},"i":10}},"\n",{"template":{"target":{"wt":"legend","href":"./Template:Legend"},"params":{"1":{"wt":"#ff6e00"},"2":{"wt":"&lt;small>3500–4000&lt;/small>"}},"i":11}},"\n",{"template":{"target":{"wt":"legend","href":"./Template:Legend"},"params":{"1":{"wt":"#ff5800"},"2":{"wt":"&lt;small>4000–4500&lt;/small>"}},"i":12}},"\n",{"template":{"target":{"wt":"legend","href":"./Template:Legend"},"params":{"1":{"wt":"#ff4200"},"2":{"wt":"