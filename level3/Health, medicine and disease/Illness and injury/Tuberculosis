<span typeof="mw:Nowiki mw:Transclusion" about="#mwt3" data-mw='{"parts":[{"template":{"target":{"wt":"good article","href":"./Template:Good_article"},"params":{},"i":0}}]}' id="mwBg"></span><link typeof="mw:Extension/indicator" about="#mwt3" data-mw='{"name":"indicator","attrs":{"name":"good-star"},"body":{"extsrc":"[[File:symbol support vote.svg|20x20px\n |link=Wikipedia:Good articles\n |\n |This is a good article. Follow the link for more information.]]\n"}}'/><link rel="mw:PageProp/Category" href="./Category:Good_articles" about="#mwt3" id="mwBw"/>
