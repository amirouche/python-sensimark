<section data-mw-section-id="36" id="mwCxM"><h3 id="Notes">Notes</h3>
<link rel="mw-deduplicated-inline-style" href="mw-data:TemplateStyles:r853264625" about="#mwt1376" typeof="mw:Extension/templatestyles mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"Refbegin","href":"./Template:Refbegin"},"params":{},"i":0}},"\n* ",{"template":{"target":{"wt":"cite web ","href":"./Template:Cite_web"},"params":{"author":{"wt":"National Asthma Education and Prevention Program"},"url":{"wt":"http://www.nhlbi.nih.gov/guidelines/asthma/asthgdln.pdf"},"title":{"wt":"Expert Panel Report 3: Guidelines for the Diagnosis and Management of Asthma"},"work":{"wt":"[[National Heart Lung and Blood Institute]]"},"year":{"wt":"2007"},"ref":{"wt":"{{harvid|NHLBI Guideline|2007}}"}},"i":1}},"\n* ",{"template":{"target":{"wt":"cite web ","href":"./Template:Cite_web"},"params":{"url":{"wt":"http://www.sign.ac.uk/pdf/sign101.pdf"},"title":{"wt":"British Guideline on the Management of Asthma"},"work":{"wt":"[[British Thoracic Society]]"},"origyear":{"wt":"2008"},"year":{"wt":"2012"},"ref":{"wt":"{{harvid|British Guideline|2009}}"}},"i":2}},"\n* ",{"template":{"target":{"wt":"cite web","href":"./Template:Cite_web"},"params":{"url":{"wt":"http://www.ginasthma.org/uploads/users/files/GINA_Report2011_May4.pdf"},"title":{"wt":"Global Strategy for Asthma Management and Prevention"},"publisher":{"wt":"Global Initiative for Asthma"},"year":{"wt":"2011"},"ref":{"wt":"{{harvid|GINA|2011}}"},"deadurl":{"wt":"yes"},"archiveurl":{"wt":"https://web.archive.org/web/20121120205023/http://www.ginasthma.org/uploads/users/files/GINA_Report2011_May4.pdf"},"archivedate":{"wt":"2016-07-01"},"df":{"wt":""}},"i":3}},"\n",{"template":{"target":{"wt":"Refend","href":"./Template:Refend"},"params":{},"i":4}}]}' id="mwCxQ"/><div class="refbegin   " style="" about="#mwt1376" id="mwCxU">
