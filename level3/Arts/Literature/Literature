<link rel="mw-deduplicated-inline-style" href="mw-data:TemplateStyles:r853264625" about="#mwt249" typeof="mw:Extension/templatestyles mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"refbegin","href":"./Template:Refbegin"},"params":{"indent":{"wt":"yes"}},"i":0}},"\n:",{"template":{"target":{"wt":"cite book","href":"./Template:Cite_book"},"params":{"last":{"wt":"Bonheim"},"first":{"wt":"Helmut"},"title":{"wt":"The Narrative Modes: Techniques of the Short Story"},"year":{"wt":"1982"},"publisher":{"wt":"Brewer"},"location":{"wt":"Cambridge"}},"i":1}}," An overview of several hundred short stories.\n:",{"template":{"target":{"wt":"cite journal","href":"./Template:Cite_journal"},"params":{"last":{"wt":"Gillespie"},"first":{"wt":"Gerald"},"title":{"wt":"Novella, nouvelle, novella, short novel? — A review of terms"},"journal":{"wt":"Neophilologus"},"date":{"wt":"January 1967"},"volume":{"wt":"51"},"issue":{"wt":"1"},"pages":{"wt":"117–127"},"doi":{"wt":"10.1007/BF01511303"}},"i":2}},"\n",{"template":{"target":{"wt":"refend","href":"./Template:Refend"},"params":{},"i":3}}]}' id="mwAws"/><div class="refbegin refbegin-hanging-indents  " style="" about="#mwt249" id="mwAww">
