<link rel="mw-deduplicated-inline-style" href="mw-data:TemplateStyles:r853264625" about="#mwt253" typeof="mw:Extension/templatestyles mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"refbegin","href":"./Template:Refbegin"},"params":{"indent":{"wt":"yes"}},"i":0}},"\n:",{"template":{"target":{"wt":"cite web","href":"./Template:Cite_web"},"params":{"last":{"wt":"Wheeler"},"first":{"wt":"L. Kip"},"title":{"wt":"Periods of Literary History"},"url":{"wt":"http://web.cn.edu/kwheeler/documents/periods_lit_history.pdf"},"publisher":{"wt":"[[Carson-Newman University]]"},"accessdate":{"wt":"18 March 2014"}},"i":1}}," Brief summary of major periods in literary history of the Western tradition.\n",{"template":{"target":{"wt":"refend","href":"./Template:Refend"},"params":{},"i":2}}]}' id="mwAw8"/><div class="refbegin refbegin-hanging-indents  " style="" about="#mwt253" id="mwAxA">
