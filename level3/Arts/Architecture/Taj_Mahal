<table class="infobox vcard" style="width:22em;width:250px" about="#mwt6" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"Infobox Historic Site\n","href":"./Template:Infobox_Historic_Site"},"params":{"name":{"wt":"Taj Mahal"},"image":{"wt":"Taj Mahal (Edited).jpeg"},"caption":{"wt":"&lt;!--no caption needed per guidelines-->"},"location":{"wt":"[[Agra]], [[Uttar Pradesh]], [[India]]"},"height":{"wt":"{{convert|73|m|ft|abbr=on}}"},"built":{"wt":"1632–53{{sfn |DuTemple |2003 |p=32|}}"},"architect":{"wt":"[[Ustad Ahmad Lahauri]]"},"architecture":{"wt":"[[Mughal architecture]]"},"coordinates":{"wt":"{{coord|27|10|30|N|78|02|31|E|display=inline,title}}"},"locmapin":{"wt":"Uttar Pradesh#India"},"map_caption":{"wt":"Location of Taj Mahal in [[Uttar pradesh]]"},"visitation_num":{"wt":"7–8&amp;nbsp;million{{sfn|Archaeological Survey of|2015}}"},"visitation_year":{"wt":"2014"},"embedded":{"wt":"{{Infobox UNESCO World Heritage Site\n |child=yes\n |ID=252\n |Year=1983\n |Criteria=Cultural: i\n }}"},"website":{"wt":"[http://www.tajmahal.gov.in/ www.tajmahal.gov.in]"},"area":{"wt":"17 hectares{{sfn|Unesco Taj Mahal|2016}}"},"built_for":{"wt":"Mumtaz Mahal"},"visitors_num":{"wt":"7-8 million"},"demolished":{"wt":""},"governing_body":{"wt":"Government of India"}},"i":0}}]}' id="mwCw"><tbody><tr><th colspan="2" class="fn org" style="text-align:center;font-size:125%;font-weight:bold;font-size:120%; text-align:center">Taj Mahal</th></tr><tr><td colspan="2" style="text-align:center"><figure-inline typeof="mw:Image"><a href="./File:Taj_Mahal_(Edited).jpeg"><img resource="./File:Taj_Mahal_(Edited).jpeg" src="//upload.wikimedia.org/wikipedia/commons/thumb/1/1d/Taj_Mahal_%28Edited%29.jpeg/250px-Taj_Mahal_%28Edited%29.jpeg" data-file-width="3840" data-file-height="2525" data-file-type="bitmap" height="164" width="250" srcset="//upload.wikimedia.org/wikipedia/commons/thumb/1/1d/Taj_Mahal_%28Edited%29.jpeg/500px-Taj_Mahal_%28Edited%29.jpeg 2x, //upload.wikimedia.org/w