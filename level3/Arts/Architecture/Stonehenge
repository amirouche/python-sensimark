<table class="infobox vcard" style="width:22em" about="#mwt7" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"Infobox ancient site\n ","href":"./Template:Infobox_ancient_site"},"params":{"name":{"wt":"Stonehenge"},"alternate_name":{"wt":""},"image":{"wt":"File:Stonehenge2007 07 30.jpg"},"alt":{"wt":""},"caption":{"wt":"Stonehenge in July 2007"},"map_type":{"wt":"United Kingdom Wiltshire"},"map_size":{"wt":""},"relief":{"wt":"yes"},"map_caption":{"wt":"Map of Wiltshire showing the location of Stonehenge"},"coordinates":{"wt":"{{coord|51|10|44|N|1|49|34|W|type:landmark_region:GB-WIL|display=inline,title}}"},"location":{"wt":"[[Wiltshire]], England"},"region":{"wt":""},"type":{"wt":"[[Monument]]"},"part_of":{"wt":""},"length":{"wt":""},"width":{"wt":""},"area":{"wt":""},"height":{"wt":"Each standing stone was around 13 foot (4.1 metres) high"},"builder":{"wt":""},"material":{"wt":"[[sarsen]], [[bluestone]]"},"built":{"wt":"[[Bronze Age]]"},"abandoned":{"wt":""},"epochs":{"wt":""},"cultures":{"wt":""},"dependency_of":{"wt":""},"occupants":{"wt":""},"event":{"wt":""},"excavations":{"wt":""},"archaeologists":{"wt":""},"condition":{"wt":""},"ownership":{"wt":""},"management":{"wt":""},"public_access":{"wt":""},"website":{"wt":"www.english-heritage.org.uk/stonehenge"},"notes":{"wt":""},"designation1":{"wt":"WHS"},"designation1_offname":{"wt":"[[Stonehenge, Avebury and Associated Sites]]"},"designation1_date":{"wt":"1986 &lt;small>(10th [[World Heritage Committee|session]])&lt;/small>"},"designation1_number":{"wt":"[http://whc.unesco.org/en/list/373 373]"},"designation1_criteria":{"wt":"i, ii, iii"},"designation1_type":{"wt":"Cultural"},"designation1_free1name":{"wt":"Region"},"designation1_free1value":{"wt":"[[List of World Heritage Sites in Europe|Europe and North America]]"}},"i":0}}]}' id="mwDQ"><caption class="fn org">Stonehenge</caption><tbody><tr><td colspan="2" style="text-align:center"><figure-inline class="mw-default-size" typeof="mw:Image/