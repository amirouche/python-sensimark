<div class="mw-collapsible  mw-collapsed " style="box-sizing:border-box;width:100%;font-size:95%;padding:4px;border:1px #aaa solid;" about="#mwt237" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"Hidden begin","href":"./Template:Hidden_begin"},"params":{"title":{"wt":"Pathophysiology of ethanol"},"titlestyle":{"wt":"text-align:center"},"border":{"wt":"1px #aaa solid"}},"i":0}},"\n",{"template":{"target":{"wt":"Antidotes","href":"./Template:Antidotes"},"params":{},"i":1}},"\n",{"template":{"target":{"wt":"Anxiolytics","href":"./Template:Anxiolytics"},"params":{},"i":2}},"\n",{"template":{"target":{"wt":"Hypnotics and sedatives","href":"./Template:Hypnotics_and_sedatives"},"params":{},"i":3}},"&lt;!-- Do not remove to avoid template duplication: These templates redirects to {{Hypnotics and sedatives}}: {{Depressants}}, {{Hypnotics}}, and {{Sedatives}} -->\n",{"template":{"target":{"wt":"Physical dependants","href":"./Template:Physical_dependants"},"params":{},"i":4}},"\n",{"template":{"target":{"wt":"Neurotoxins","href":"./Template:Neurotoxins"},"params":{},"i":5}},"\n",{"template":{"target":{"wt":"Hidden end","href":"./Template:Hidden_end"},"params":{},"i":6}}]}' id="mwA2M"><div style="font-size:100%;line-height:1.6;font-weight:bold;background:transparent;text-align:left;;text-align:center">Pathophysiology of ethanol</div><div class="mw-collapsible-content" style="background:transparent;text-align:left;;">
