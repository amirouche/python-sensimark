<div class="mw-collapsible  mw-collapsed " style="box-sizing:border-box;width:100%;font-size:95%;padding:4px;border:1px #aaa solid;" about="#mwt244" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"Hidden begin","href":"./Template:Hidden_begin"},"params":{"title":{"wt":"Pharmacology of ethanol"},"titlestyle":{"wt":"text-align:center"},"border":{"wt":"1px #aaa solid"}},"i":0}},"\n",{"template":{"target":{"wt":"Acetylcholine receptor modulators","href":"./Template:Acetylcholine_receptor_modulators"},"params":{},"i":1}},"\n",{"template":{"target":{"wt":"GABAA receptor positive modulators","href":"./Template:GABAA_receptor_positive_modulators"},"params":{},"i":2}},"\n",{"template":{"target":{"wt":"Glycine receptor modulators","href":"./Template:Glycine_receptor_modulators"},"params":{},"i":3}},"\n",{"template":{"target":{"wt":"Ionotropic glutamate receptor modulators","href":"./Template:Ionotropic_glutamate_receptor_modulators"},"params":{},"i":4}},"\n",{"template":{"target":{"wt":"Serotonin receptor modulators","href":"./Template:Serotonin_receptor_modulators"},"params":{},"i":5}},"\n",{"template":{"target":{"wt":"Hidden end","href":"./Template:Hidden_end"},"params":{},"i":6}}]}' id="mwA2Q"><div style="font-size:100%;line-height:1.6;font-weight:bold;background:transparent;text-align:left;;text-align:center">Pharmacology of ethanol</div><div class="mw-collapsible-content" style="background:transparent;text-align:left;;">
