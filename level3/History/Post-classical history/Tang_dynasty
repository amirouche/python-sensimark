 Bulletin"},"volume":{"wt":"86"},"issue":{"wt":"2"},"pages":{"wt":"228–254"},"jstor":{"wt":"3177416"}},"i":63}},"\n* ",{"template":{"target":{"wt":"citation ","href":"./Template:Citation"},"params":{"last":{"wt":"Sun"},"first":{"wt":"Guangqi"},"title":{"wt":"History of Navigation in Ancient China"},"year":{"wt":"1989"},"publisher":{"wt":"Ocean Press"},"location":{"wt":"Beijing"},"isbn":{"wt":"978-7-5027-0532-9"}},"i":64}},"\n* ",{"template":{"target":{"wt":"citation ","href":"./Template:Citation"},"params":{"last":{"wt":"Taenzer"},"first":{"wt":"Gertraud"},"chapter":{"wt":"Changing Relations between Administration, Clergy and Lay People in Eastern Central Asia: a Case Study According to the Dunhuang Manuscripts Referring to the Transition from Tibetan to Local Rule in Dunhuang, 8th–11th Centuries"},"title":{"wt":"Transfer of Buddhism Across Central Asian Networks (7th to 13th Centuries)"},"editor":{"wt":"Carmen Meinert"},"year":{"wt":"2016"},"publisher":{"wt":"Brill"},"location":{"wt":"Leiden; Boston"},"isbn":{"wt":"978-90-04-30741-4"},"pages":{"wt":"19–56"}},"i":65}},"\n* ",{"template":{"target":{"wt":"Citation ","href":"./Template:Citation"},"params":{"last":{"wt":"Tang"},"first":{"wt":"Zhiba"},"year":{"wt":"1991"},"title":{"wt":"The influence of the sail on the development of the ancient navy"},"work":{"wt":"Proceedings of the International Sailing Ships Conference in Shanghai"}},"i":66}},"\n* ",{"template":{"target":{"wt":"citation ","href":"./Template:Citation"},"params":{"last":{"wt":"Temple"},"first":{"wt":"Robert"},"others":{"wt":"with a foreword by Joseph Needham"},"title":{"wt":"The Genius of China: 3,000 Years of Science, Discovery, and Invention"},"year":{"wt":"1986"},"publisher":{"wt":"Simon and Schuster"},"location":{"wt":"New York"},"isbn":{"wt":"978-0-671-62028-8"}},"i":67}},"\n* ",{"template":{"target":{"wt":"cite book ","href":"./Template:Cite_book"},"params":{"last":{"wt":"Togan"},"first":{"wt":"Isenbike"},"chapter":{"wt":"Court Historiography in 