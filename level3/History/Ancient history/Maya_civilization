ouse\""},"journal":{"wt":"American Anthropologist"},"volume":{"wt":"102"},"issue":{"wt":"3"},"pages":{"wt":"467–484"},"jstor":{"wt":"683405"},"issn":{"wt":"0002-7294"},"oclc":{"wt":"1479294"},"subscription":{"wt":"y"}},"i":46}},"\n:",{"template":{"target":{"wt":"cite book ","href":"./Template:Cite_book"},"params":{"author":{"wt":"Guillemín, Jorge F."},"year":{"wt":"1965"},"title":{"wt":"Iximché: Capital del Antiguo Reino Cakchiquel"},"publisher":{"wt":"Tipografía Nacional de Guatemala"},"location":{"wt":"Guatemala City, Guatemala"},"oclc":{"wt":"1498320"},"language":{"wt":"es"},"trans-title":{"wt":"Iximche: Capital of the Ancient Kaqchikel Kingdom"}},"i":47}},"\n:",{"template":{"target":{"wt":"cite journal ","href":"./Template:Cite_journal"},"params":{"author1":{"wt":"Hammond, Norman"},"author2":{"wt":"Duncan Pring"},"author3":{"wt":"Rainer Berger"},"author4":{"wt":"V. R. Switsur"},"author5":{"wt":"A. P. Ward"},"url":{"wt":"http://www.nature.com/nature/journal/v260/n5552/abs/260579a0.html"},"title":{"wt":"Radiocarbon chronology for early Maya occupation at Cuello, Belize"},"journal":{"wt":"Nature"},"issue":{"wt":"260"},"doi":{"wt":"10.1038/260579a0"},"date":{"wt":"1976-04-15"},"issn":{"wt":"0028-0836"},"accessdate":{"wt":"2010-08-01"},"volume":{"wt":"260"},"pages":{"wt":"579–81"},"bibcode":{"wt":"1976Natur.260..579H"}},"i":48}},"\n: ",{"template":{"target":{"wt":"cite journal","href":"./Template:Cite_journal"},"params":{"author":{"wt":"Hansen, Richard D."},"authorlink":{"wt":"Richard D. Hansen"},"year":{"wt":"1991"},"title":{"wt":"Resultados preliminares de las investigaciones arqueológicas en el sitio Nakbe, Petén, Guatemala"},"trans-title":{"wt":"Preliminary Results of the Archaeological Investigations of Nakbe, Peten, Guatemala"},"url":{"wt":"http://www.asociaciontikal.com/pdf/23.88%20-%20Richard%20Hansen%20-%20en%20PDF.pdf"},"format":{"wt":"PDF"},"volume":{"wt":"II (1988)"},"journal":{"wt":"Simposio de Investigaciones Arqueológicas en Guatemala"},"editor1":{"wt"