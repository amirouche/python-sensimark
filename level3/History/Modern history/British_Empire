ef="./Category:Articles_with_short_description" about="#mwt1" id="mwAw"/>
<p id="mwBA"><span typeof="mw:Nowiki mw:Transclusion" about="#mwt2" data-mw='{"parts":[{"template":{"target":{"wt":"pp","href":"./Template:Pp"},"params":{"small":{"wt":"yes"}},"i":0}}]}' id="mwBQ"></span><link typeof="mw:Extension/indicator" about="#mwt2" data-mw='{"name":"indicator","attrs":{"name":"pp-default"},"body":{"extsrc":"[[File:Semi-protection-shackle.svg|20px|link=Wikipedia:Protection policy#semi|alt=Page semi-protected|This article is semi-protected.]]"}}'/><link rel="mw:PageProp/Category" href="./Category:Wikipedia_indefinitely_semi-protected_pages#British%20Empire" about="#mwt2" id="mwBg"/>
