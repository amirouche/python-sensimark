        <mrow class="MJX-TeXAtom-ORD">
          <mfrac>
            <mn>1</mn>
            <mn>2</mn>
          </mfrac>
        </mrow>
        <mo stretchy="false">(</mo>
        <mi>A</mi>
        <mi>v</mi>
        <mi>t</mi>
        <mi>ρ<!-- ρ --></mi>
        <mo stretchy="false">)</mo>
        <msup>
          <mi>v</mi>
          <mrow class="MJX-TeXAtom-ORD">
            <mn>2</mn>
          </mrow>
        </msup>
