        <mrow class="MJX-TeXAtom-ORD">
          <mfrac>
            <mn>1</mn>
            <mn>2</mn>
          </mfrac>
        </mrow>
        <mi>m</mi>
        <msup>
          <mi>v</mi>
          <mrow class="MJX-TeXAtom-ORD">
            <mn>2</mn>
