</section><section data-mw-section-id="69" id="mwDyc"><h3 id="History_of_AI">History of AI</h3>
<link rel="mw-deduplicated-inline-style" href="mw-data:TemplateStyles:r853264625" about="#mwt1824" typeof="mw:Extension/templatestyles mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"refbegin","href":"./Template:Refbegin"},"params":{"1":{"wt":"30em"}},"i":0}},"\n* ",{"template":{"target":{"wt":"Crevier 1993","href":"./Template:Crevier_1993"},"params":{},"i":1}},".\n* ",{"template":{"target":{"wt":"McCorduck 2004","href":"./Template:McCorduck_2004"},"params":{},"i":2}},".\n* ",{"template":{"target":{"wt":"cite book\n","href":"./Template:Cite_book"},"params":{"last":{"wt":"Newquist"},"first":{"wt":"HP"},"author-link":{"wt":"HP Newquist"},"year":{"wt":"1994"},"title":{"wt":"The Brain Makers: Genius, Ego, And Greed In The Quest For Machines That Think"},"publisher":{"wt":"Macmillan/SAMS"},"location":{"wt":"New York"},"isbn":{"wt":"978-0-672-30412-5"}},"i":3}},"\n* ",{"template":{"target":{"wt":"cite book\n","href":"./Template:Cite_book"},"params":{"last":{"wt":"Nilsson"},"first":{"wt":"Nils"},"author-link":{"wt":"Nils Nilsson (researcher)"},"year":{"wt":"2009"},"title":{"wt":"The Quest for Artificial Intelligence: A History of Ideas and Achievements"},"publisher":{"wt":"Cambridge University Press"},"location":{"wt":"New York"},"isbn":{"wt":"978-0-521-12293-1"}},"i":4}},"\n",{"template":{"target":{"wt":"refend","href":"./Template:Refend"},"params":{},"i":5}}]}' id="mwDyg"/><div class="refbegin   columns references-column-width" style="-moz-column-width: 30em; -webkit-column-width: 30em; column-width: 30em;" about="#mwt1824" id="mwDyk">
