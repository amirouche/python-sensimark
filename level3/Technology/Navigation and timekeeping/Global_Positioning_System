            <mo>+</mo>
            <mn>1</mn>
            <mo>,</mo>
            <mi>k</mi>
          </mrow>
        </msub>
        <mo>−<!-- − --></mo>
        <msub>
          <mi>α<!-- α --></mi>
          <mrow class="MJX-TeXAtom-ORD">
            <mi>i</mi>
