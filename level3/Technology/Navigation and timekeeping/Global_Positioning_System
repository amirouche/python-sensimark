<td id="mwvw">1997–2004</td><td id="mwwA">12</td><td id="mwwQ">1</td><td id="mwwg">0</td><td id="mwww">0</td><td id="mwxA">12</td></tr>
<tr id="mwxQ">
<th id="mwxg"><a rel="mw:WikiLink" href="./GPS_Block_IIR-M" title="GPS Block IIR-M" id="mwxw" class="mw-redirect">IIR-M</a></th>
<td id="mwyA">2005–2009</td><td id="mwyQ">8</td><td id="mwyg">0</td><td id="mwyw">0</td><td id="mwzA">0</td><td id="mwzQ">7</td></tr>
