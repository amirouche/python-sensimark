<td id="mwA0s">Racing car</td><td id="mwA0w"><a rel="mw:WikiLink" href="./Formula_One_car" title="Formula One car" id="mwA00">Formula One car</a> 3.0<span typeof="mw:Entity" id="mwA04"> </span>L</td>
<td id="mwA08"><span about="#mwt290" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"convert","href":"./Template:Convert"},"params":{"1":{"wt":"950"},"2":{"wt":"hp"},"3":{"wt":"kW"},"order":{"wt":"flip"},"abbr":{"wt":"on"}},"i":0}}]}' id="mwA1A">710</span><span typeof="mw:Entity" about="#mwt290"> </span><span about="#mwt290">kW (950</span><span typeof="mw:Entity" about="#mwt290"> </span><span about="#mwt290">hp)</span></td>
<td id="mwA1E"><span about="#mwt291" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"convert","href":"./Template:Convert"},"params":{"1":{"wt":"1428"},"2":{"wt":"hp/t"},"3":{"wt":"kW/t"},"order":{"wt":"flip"},"abbr":{"wt":"on"}},"i":0}}]}' id="mwA1I">1,065</span><span typeof="mw:Entity" about="#mwt291"> </span><span about="#mwt291">kW/t (1,428</span><span typeof="mw:Entity" about="#mwt291"> </span><span about="#mwt291">hp/t)</span></td>
<td id="mwA1M"><span about="#mwt292" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"convert","href":"./Template:Convert"},"params":{"1":{"wt":"350"},"2":{"wt":"Nm"},"abbr":{"wt":"on"}},"i":0}}]}' id="mwA1Q">350</span><span typeof="mw:Entity" about="#mwt292"> </span><span about="#mwt292">N⋅m (260</span><span typeof="mw:Entity" about="#mwt292"> </span><span about="#mwt292">lbf⋅ft)</span></td></tr>
<tr id="mwA1U">
<td id="mwA1Y">Main battle tank</td><td id="mwA1c"><a rel="mw:WikiLink" href="./Leopard_2" title="Leopard 2" id="mwA1g">Leopard 2</a>, <a rel="mw:WikiLink" href="./M1_Abrams" title="M1 Abrams" id="mwA1k">M1 Abrams</a></td>
