              <mi>t</mi>
            </mrow>
          </mfrac>
        </mrow>
        <mo>&lt;</mo>
        <mn>0</mn>
        <mo>,</mo>
        <mrow class="MJX-TeXAtom-ORD">
          <mfrac>
            <mrow>
