              <mi mathvariant="normal">d</mi>
            </mrow>
            <mrow>
              <mrow class="MJX-TeXAtom-ORD">
                <mi mathvariant="normal">d</mi>
              </mrow>
              <mi>t</mi>
            </mrow>
          </mfrac>
        </mrow>
        <mrow class="MJX-TeXAtom-ORD">
          <mrow class="MJX-TeXAtom-ORD">
            <mo maxsize="1.2em" minsize="1.2em">[</mo>
          </mrow>
        </mrow>
        <mi>m</mi>
        <mo stretchy="false">(</mo>
        <mi>t</mi>
        <mo stretchy="false">)</mo>
