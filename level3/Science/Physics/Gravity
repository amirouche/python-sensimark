<div style="clear:both;" about="#mwt159" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"clear","href":"./Template:Clear"},"params":{},"i":0}}]}' id="mwAeQ"></div>

</section><section data-mw-section-id="21" id="mwAeU"><h2 id="Footnotes">Footnotes</h2>
<div class="reflist columns references-column-width" style="-moz-column-width: 30em; -webkit-column-width: 30em; column-width: 30em; list-style-type: decimal;" about="#mwt160" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"Reflist","href":"./Template:Reflist"},"params":{"1":{"wt":"30em"}},"i":0}}]}' id="mwAeY">
