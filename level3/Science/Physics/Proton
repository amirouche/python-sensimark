                          <msub>
                            <mi>G</mi>
                            <mrow class="MJX-TeXAtom-ORD">
                              <mi>e</mi>
                            </mrow>
                          </msub>
                        </mrow>
                        <mrow>
                          <mi>d</mi>
                          <msup>
                            <mi>q</mi>
                            <mrow class="MJX-TeXAtom-ORD">
                              <mn>2</mn>
                            </mrow>
                          </msup>
                        </mrow>
                      </mfrac>
                    </mrow>
                    <mspace width="thinmathspace"></mspace>
                    <mrow class="MJX-TeXAtom-ORD">
