<tr style="background: #fff4e8" about="#mwt1411" typeof="mw:ExpandedAttrs" data-mw='{"attribs":[[{"txt":"style"},{"html":"background: &lt;span typeof=\"mw:Nowiki mw:Transclusion\" about=\"#mwt506\" data-parsoid=&apos;{\"pi\":[[{\"k\":\"1\"}]],\"dsr\":[106211,106227]}&apos; data-mw=&apos;{\"parts\":[{\"template\":{\"target\":{\"wt\":\"star-color\",\"href\":\"./Template:Star-color\"},\"params\":{\"1\":{\"wt\":\"G\"}},\"i\":0}}]}&apos;>#&lt;/span>&lt;span about=\"#mwt506\" data-parsoid=\"{}\">fff4e8&lt;/span>"}]]}' id="mwA3A">
