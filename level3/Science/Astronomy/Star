<tr style="background: #ffddb4" about="#mwt1412" typeof="mw:ExpandedAttrs" data-mw='{"attribs":[[{"txt":"style"},{"html":"background: &lt;span typeof=\"mw:Nowiki mw:Transclusion\" about=\"#mwt507\" data-parsoid=&apos;{\"pi\":[[{\"k\":\"1\"}]],\"dsr\":[106286,106302]}&apos; data-mw=&apos;{\"parts\":[{\"template\":{\"target\":{\"wt\":\"star-color\",\"href\":\"./Template:Star-color\"},\"params\":{\"1\":{\"wt\":\"K\"}},\"i\":0}}]}&apos;>#&lt;/span>&lt;span about=\"#mwt507\" data-parsoid=\"{}\">ffddb4&lt;/span>"}]]}' id="mwA3Y">
<td id="mwA3c">K</td>
