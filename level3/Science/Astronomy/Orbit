            </mover>
          </mrow>
        </mrow>
        <mo>−<!-- − --></mo>
        <mi>r</mi>
        <msup>
          <mrow class="MJX-TeXAtom-ORD">
            <mrow class="MJX-TeXAtom-ORD">
              <mover>
                <mi>θ<!-- θ --></mi>
                <mo>˙<!-- ˙ --></mo>
              </mover>
            </mrow>
          </mrow>
          <mrow class="MJX-TeXAtom-ORD">
            <mn>2</mn>
          </mrow>
        </msup>
        <mo>=</mo>
        <mo>−<!-- − --></mo>
        <mrow class="MJX-TeXAtom-ORD">
          <mfrac>
            <mi>μ<!-- μ --></mi>
            <msup>
              <mi>r</mi>
              <mrow class="MJX-TeXAtom-ORD">
