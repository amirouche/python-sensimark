                <mi id="mwAyY">J</mi>
                <mrow class="MJX-TeXAtom-ORD" id="mwAyc">
                  <mo id="mwAyg">/</mo>
                </mrow>
                <mi id="mwAyk">M</mi>
                <mo stretchy="false" id="mwAyo">)</mo>
              </mrow>
              <mrow class="MJX-TeXAtom-ORD" id="mwAys">
