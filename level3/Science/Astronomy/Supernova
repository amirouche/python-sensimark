<cite class="citation journal" about="#mwt60" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"cite journal\n ","href":"./Template:Cite_journal"},"params":{"last1":{"wt":"Leibundgut"},"first1":{"wt":"B."},"last2":{"wt":"Aldering"},"first2":{"wt":"G."},"last3":{"wt":"Amanullah"},"first3":{"wt":"R."},"last4":{"wt":"Astier"},"first4":{"wt":"P."},"last5":{"wt":"Blanc"},"first5":{"wt":"G."},"last6":{"wt":"Burns"},"first6":{"wt":"M. S."},"last7":{"wt":"Conley"},"first7":{"wt":"A. J."},"last8":{"wt":"Deustua"},"first8":{"wt":"S. E."},"last9":{"wt":"Doi"},"first9":{"wt":"M."},"last10":{"wt":"Ellis"},"first10":{"wt":"R."},"last11":{"wt":"Fabbro"},"first11":{"wt":"S."},"last12":{"wt":"Folatelli"},"first12":{"wt":"G."},"last13":{"wt":"Fruchter"},"first13":{"wt":"A. S."},"last14":{"wt":"Garavini"},"first14":{"wt":"G."},"last15":{"wt":"Garmond"},"first15":{"wt":"S."},"last16":{"wt":"Garton"},"first16":{"wt":"K."},"last17":{"wt":"Gibbons"},"first17":{"wt":"R."},"last18":{"wt":"Goldhaber"},"first18":{"wt":"G."},"last19":{"wt":"Goobar"},"first19":{"wt":"A."},"last20":{"wt":"Groom"},"first20":{"wt":"D. E."},"last21":{"wt":"Hardin"},"first21":{"wt":"D."},"last22":{"wt":"Hook"},"first22":{"wt":"I. M."},"last23":{"wt":"Howell"},"first23":{"wt":"D. A."},"last24":{"wt":"Kim"},"first24":{"wt":"A. G."},"last25":{"wt":"Lee"},"first25":{"wt":"B. C."},"last26":{"wt":"Lidman"},"first26":{"wt":"C."},"last27":{"wt":"Mendez"},"first27":{"wt":"J."},"last28":{"wt":"Nobili"},"first28":{"wt":"S."},"last29":{"wt":"Nugent"},"first29":{"wt":"P. E."},"last30":{"wt":"Pain"},"first30":{"wt":"R."},"display-authors":{"wt":"1"},"year":{"wt":"2003"},"title":{"wt":"A cosmological surprise: The universe accelerates"},"journal":{"wt":"The Astrophysical Journal"},"volume":{"wt":"32"},"issue":{"wt":"4"},"pages":{"wt":"121"},"bibcode":{"wt":"2001ENews..32..121L"},"doi":{"wt":"10.1086/378560"},"arxiv":{"wt":"astro-ph/0309368"}},"i":0}}]}' id="mwBKE">Leibundgut, B.;  et al. (2003). "A cosmologi