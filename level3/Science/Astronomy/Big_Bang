<span typeof="mw:Nowiki mw:Transclusion" about="#mwt4" data-mw='{"parts":[{"template":{"target":{"wt":"pp-semi-indef","href":"./Template:Pp-semi-indef"},"params":{},"i":0}}]}' id="mwCA"></span><link typeof="mw:Extension/indicator" about="#mwt4" data-mw='{"name":"indicator","attrs":{"name":"pp-default"},"body":{"extsrc":"[[File:Semi-protection-shackle.svg|20px|link=Wikipedia:Protection policy#semi|alt=Page semi-protected|This article is semi-protected.]]"}}'/><link rel="mw:PageProp/Category" href="./Category:Wikipedia_indefinitely_semi-protected_pages#Big%20Bang" about="#mwt4" id="mwCQ"/>
