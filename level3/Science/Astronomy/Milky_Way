<table class="infobox" style="width:22em" about="#mwt26" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"Infobox\n","href":"./Template:Infobox"},"params":{"title":{"wt":"Milky Way Galaxy"},"image":{"wt":"[[File:ESO-VLT-Laser-phot-33a-07.jpg|300px]]"},"caption":{"wt":"{{longitem|\nThe Milky Way&apos;s [[Galactic Center]] in the night sky above the [[Paranal Observatory]] (the laser creates a [[Laser guide star|guide-star]] for the telescope)|style=padding: 4px 0;}}"},"titlestyle":{"wt":"background: #30D5C8;"},"headerstyle":{"wt":"background:"},"belowstyle":{"wt":"background: #30D5C8;"},"labelstyle":{"wt":"background: inherit;"},"header1":{"wt":"Observation data"},"label2":{"wt":"[[Galaxy morphological classification|Type]]"},"data2":{"wt":"Sb, Sbc, or SB(rs)bc&lt;ref name=ssr100_1_129/>&lt;ref>{{cite web|first1=Hartmut |last1=Frommert |first2=Christine |last2=Kronberg |date=August 26, 2005 |url=http://messier.seds.org/more/mw_type.html |title=Classification of the Milky Way Galaxy |accessdate=May 30, 2015 |work=SEDS |deadurl=no |archiveurl=https://web.archive.org/web/20150531031937/http://messier.seds.org/more/mw_type.html |archivedate=May 31, 2015 }}&lt;/ref> ([[barred spiral galaxy]])"},"label3":{"wt":"[[Diameter]]"},"data3":{"wt":"{{Convert|150-200|kly|kpc|abbr=on|lk=on}}"},"label4":{"wt":"Thickness of thin stellar disk"},"data4":{"wt":"≈{{Convert|2|kly|kpc|abbr=on|lk=off|sigfig=1}}&lt;ref name=\"ask-astro\"/>&lt;ref name=\"Rix_Bovy\" />"},"label6":{"wt":"Number of stars"},"data6":{"wt":"{{nowrap|100–400 billion [(1–4)×10&lt;sup>11&lt;/sup>]}}&lt;ref name=odenwald/>"},"label7":{"wt":""},"data7":{"wt":""},"label8":{"wt":"Mass"},"data8":{"wt":"0.8–1.5{{e|12}}&amp;nbsp;{{solar mass|link=yes}}&lt;ref name=\"McMillan2011\"/>&lt;ref name=\"McMillan2016\"/>&lt;ref name=\"Kafle2012\"/>&lt;ref name=\"Kafle2014\"/>"},"label9":{"wt":"Angular momentum"},"data9":{"wt":"≈{{val|1|e=67|u=J s}}&lt;ref>{{cite web|last1=Karachentsev |first1=Igor |title=Double Galaxies §7.1 |url=https://ned.ipac.caltech.edu/level5/Sept02/Keel/Keel7.html |website=ned.ipac.caltech.edu |publisher=Izdatel&apos;stvo Nauka |accessdate=April 5, 2015 |deadurl=no |archiveurl=https://web.archive.org/web/20160304054350/https://ned.ipac.caltech.edu/level5/Sept02/Keel/Keel7.html |archivedate=March 4, 2016 }}&lt;/ref>"},"label10":{"wt":"Sun&apos;s distance to [[Galactic Center]]"},"data10":{"wt":"{{convert|26.4|±|1.0|kly|kpc|abbr=on|lk=off}}&lt;ref name=\"apj692_2_1075\"/>&lt;ref name=\"boehle2016\"/>&lt;ref name=\"Gillessen2016\"/>"},"label11":{"wt":"Sun&apos;s [[Galactic Year|Galactic rotation period]]"},"data11":{"wt":"240&amp;nbsp;[[Myr]]&lt;ref name=\"sparke_gallagher\"/>"},"label12":{"wt":"[[Density wave theory|Spiral pattern rotation period]]"},"data12":{"wt":"220–360&amp;nbsp;Myr&lt;ref name=rotation_pattern_speeds/>"},"label13":{"wt":"[[Barred spiral galaxy|Bar pattern]] rotation period"},"data13":{"wt":"100–120&amp;nbsp;Myr&lt;ref name=rotation_pattern_speeds/>"},"label14":{"wt":"Speed relative to [[Cosmic microwave background|CMB]] [[rest frame]]"},"data14":{"wt":"{{nowrap|631 ± 20 km/s}}&lt;ref>{{cite article|url=http://www.nature.com/articles/s41550-016-0036 |title=The dipole repeller |author=Yehuda Hoffman, Daniel Pomarède, R. Brent Tully &amp; Hélène M. Courtois |work=Nature Astronomy |doi=10.1038/s41550-016-0036 |date=August 22, 2016 |deadurl=no |archiveurl=https://web.archive.org/web/20170303111934/http://www.nature.com/articles/s41550-016-0036 |archivedate=March 3, 2017 |arxiv = 1702.02483 |bibcode = 2017NatAs...1E..36H }}&lt;/ref>"},"below":{"wt":"See also: [[Galaxy]], [[List of galaxies]]"},"label15":{"wt":"Escape velocity at Sun&apos;s position"},"data15":{"wt":"550 km/s&lt;ref name=\"Kafle2014\"/>"},"label16":{"wt":"Dark matter density at Sun&apos;s position"},"data16":{"wt":"0.0088{{sup sub|+0.0024 |−0.0018}} {{solar mass|link=yes}}&lt;nowiki/>pc{{sup|-3}} or 0.35{{sup sub|+0.08|−0.07}} GeV cm{{sup|-3}}&lt;ref name=\"Kafle2014\"/>"}},"i":0}}]}' id="mwDQ"><caption style="background: #30D5C8;">Milky Way Galaxy</caption><tbody><tr><td colspan="2" style="text-align:center"><figure-inline typeof="mw:Image"><a href="./File:ESO-VLT-Laser-phot-33a-07.jpg"><img resource="./File:ESO-VLT-Laser-phot-33a-07.jpg" src="//upload.wikimedia.org/wikipedia/commons/thumb/4/43/ESO-VLT-Laser-phot-33a-07.jpg/300px-ESO-VLT-Laser-phot-33a-07.jpg" data-file-width="3888" data-file-height="2583" data-file-type="bitmap" height="199" width="300" srcset="//upload.wikimedia.org/wikipedia/commons/thumb/4/43/ESO-VLT-Laser-phot-33a-07.jpg/600px-ESO-VLT-Laser-phot-33a-07.jpg 2x, //upload.wikimedia.org/wikipedia/commons/thumb/4/43/ESO-VLT-Laser-phot-33a-07.jpg/450px-ESO-VLT-Laser-phot-33a-07.jpg 1.5x"/></a></figure-inline><div><div style="padding:0.1em 0;line-height:1.2em;padding: 4px 0;">
