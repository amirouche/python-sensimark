where <span class="mwe-math-element" typeof="mw:Extension/math" about="#mwt1384" data-mw='{"name":"math","attrs":{},"body":{"extsrc":"\\beta_g"}}' id="mwA9g"><span class="mwe-math-mathml-inline mwe-math-mathml-a11y" style="display: none;" id="mwA9k"><math xmlns="http://www.w3.org/1998/Math/MathML" alttext="{\displaystyle \beta _{g}}" id="mwA9o">
