<td style="background:#f87b05;"><span title="Rubidium">Rb</span></td><td style="background:#667f00;"><span title="Strontium">Sr</span></td><td>Y</td><td><span typeof="mw:Entity"> </span></td><td>Zr</td><td>Nb</td><td style="background:#7f0;"><span title="Molybdenum">Mo</span></td><td>Tc</td><td>Ru</td><td>Rh</td><td>Pd</td><td>Ag</td><td style="background:#f87b05;"><span title="Cadmium">Cd</span></td><td>In</td><td style="background:#f87b05;"><span title="Tin">Sn</span></td><td>Sb</td><td>Te</td><td style="background:#7f0;"><span title="Iodine">I</span></td><td>Xe</td></tr>
<tr>
<td>Cs</td><td>Ba</td><td style="background:#ffc;"><span title="Lanthanum">La</span></td><td>*</td><td>Hf</td><td>Ta</td><td style="background:#ffc;"><span title="Tungsten">W</span></td><td>Re</td><td>Os</td><td>Ir</td><td>Pt</td><td>Au</td><td>Hg</td><td>Tl</td><td style="background:#f87b05;"><span title="Lead">Pb</span></td><td>Bi</td><td>Po</td><td>At</td><td>Rn</td></tr>
