y"},"first4":{"wt":"Jane V."},"last5":{"wt":"Neidle"},"first5":{"wt":"Stephen"}},"i":8}},"\n* ",{"template":{"target":{"wt":"cite book ","href":"./Template:Cite_book"},"params":{"ref":{"wt":"Eldra"},"author1":{"wt":"Eldra P. Solomon"},"author2":{"wt":"Linda R. Berg"},"author3":{"wt":"Diana W. Martin"},"title":{"wt":"Biology, 8th Edition, International Student Edition"},"publisher":{"wt":"Thomson Brooks/Cole"},"isbn":{"wt":"978-0-495-31714-2"},"url":{"wt":"http://www.slideshare.net/nicolledb05/biology-solomon-berg-martin-8th-edition"},"year":{"wt":"2007"},"deadurl":{"wt":"yes"},"archiveurl":{"wt":"https://web.archive.org/web/20160304064412/http://www.slideshare.net/nicolledb05/biology-solomon-berg-martin-8th-edition"},"archivedate":{"wt":"2016-03-04"},"df":{"wt":""}},"i":9}},"\n* ",{"template":{"target":{"wt":"cite journal ","href":"./Template:Cite_journal"},"params":{"ref":{"wt":"Fariselli"},"doi":{"wt":"10.1093/bib/bbl032"},"pmid":{"wt":"17003074"},"title":{"wt":"The WWWH of remote homolog detection: The state of the art"},"journal":{"wt":"Briefings in Bioinformatics"},"volume":{"wt":"8"},"issue":{"wt":"2"},"pages":{"wt":"78–87"},"year":{"wt":"2006"},"last1":{"wt":"Fariselli"},"first1":{"wt":"P."},"last2":{"wt":"Rossi"},"first2":{"wt":"I."},"last3":{"wt":"Capriotti"},"first3":{"wt":"E."},"last4":{"wt":"Casadio"},"first4":{"wt":"R."}},"i":10}}," ",{"template":{"target":{"wt":"open access","href":"./Template:Open_access"},"params":{},"i":11}},"\n* ",{"template":{"target":{"wt":"cite book ","href":"./Template:Cite_book"},"params":{"ref":{"wt":"Fiske"},"last1":{"wt":"Fiske"},"first1":{"wt":"John"},"title":{"wt":"Outlines of Cosmic Philosophy Based on the Doctrines of Evolution, with Criticisms on the Positive Philosophy, Volume 1"},"date":{"wt":"1890"},"publisher":{"wt":"Houghton, Mifflin"},"location":{"wt":"Boston and New York"},"url":{"wt":"https://books.google.com/books?id=bTgmlJNazxkC&amp;printsec=frontcover#v=onepage&amp;q&amp;f=false"},"accessdate":{"wt":"16 Febr