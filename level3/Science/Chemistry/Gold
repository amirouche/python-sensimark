<td align="left" id="mwA4c"><b id="mwA4g">Total</b></td><td id="mwA4k"><b id="mwA4o">1508.70</b></td><td id="mwA4s"><b id="mwA4w">1805.60</b></td><td id="mwA40"></td><td id="mwA44"><b id="mwA48"> </b></td></tr>
<tr id="mwA5A">
<td align="left" id="mwA5E"><i id="mwA5I">Other Countries</i></td><td id="mwA5M"><i id="mwA5Q">251.6</i></td><td id="mwA5U"><i id="mwA5Y">254.0</i></td><td id="mwA5c"><i id="mwA5g">390.4</i></td><td id="mwA5k"><i id="mwA5o">393.5</i></td><td id="mwA5s"><i id="mwA5w">450.7</i></td></tr>
