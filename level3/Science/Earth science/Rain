<td style="text-align:right;" about="#mwt584">11,872<span typeof="mw:Entity"> </span></td>
<td id="mwAsw"><span typeof="mw:Entity" id="mwAs0"> </span><a rel="mw:WikiLink" href="./Mawsynram" title="Mawsynram" id="mwAs4">Mawsynram</a>, <a rel="mw:WikiLink" href="./India" title="India" id="mwAs8">India</a><sup class="reference" id="ref_ab" about="#mwt585" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"ref label","href":"./Template:Ref_label"},"params":{"1":{"wt":"a"},"2":{"wt":"a"},"3":{"wt":"b"}},"i":0}}]}'><a rel="mw:WikiLink" href="./Rain#endnote_ab">[a]</a></sup><sup class="reference" id="ref_dnone" about="#mwt586" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"ref label","href":"./Template:Ref_label"},"params":{"1":{"wt":"d"},"2":{"wt":"d"},"3":{"wt":"none"}},"i":0}}]}'><a rel="mw:WikiLink" href="./Rain#endnote_dnone">[d]</a></sup><span typeof="mw:Entity" id="mwAtA"> </span></td>
