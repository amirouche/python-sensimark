<td style="text-align:right;" about="#mwt587">1,401<span typeof="mw:Entity"> </span></td><td id="mwAtI"><span typeof="mw:Entity" id="mwAtM"> </span>39<span typeof="mw:Entity" id="mwAtQ"> </span></td></tr>
<tr id="mwAtU">
<td id="mwAtY"><span typeof="mw:Entity" id="mwAtc"> </span><a rel="mw:WikiLink" href="./Africa" title="Africa" id="mwAtg">Africa</a><span typeof="mw:Entity" id="mwAtk"> </span></td>
