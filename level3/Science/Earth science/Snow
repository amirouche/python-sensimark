<th id="mwqg"><i id="mwqw">above</i> saturation</th></tr>
<tr id="mwrA">
<td about="#mwt96" typeof="mw:Transclusion" style="text-align:right;" data-mw='{"parts":["| ",{"template":{"target":{"wt":"convert","href":"./Template:Convert"},"params":{"1":{"wt":"0"},"2":{"wt":"to"},"3":{"wt":"-3.5"},"4":{"wt":"C"},"5":{"wt":"F"},"6":{"wt":"0"},"disp":{"wt":"table"}},"i":0}}]}' id="mwrQ">0 to −3.5</td>
<td style="text-align:right;" about="#mwt96">32 to 26</td>
