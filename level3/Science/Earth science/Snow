<tr id="mwwA">
<td about="#mwt102" typeof="mw:Transclusion" style="text-align:right;" data-mw='{"parts":["| ",{"template":{"target":{"wt":"convert","href":"./Template:Convert"},"params":{"1":{"wt":"-22"},"2":{"wt":"to"},"3":{"wt":"-40"},"4":{"wt":"C"},"5":{"wt":"F"},"6":{"wt":"0"},"disp":{"wt":"table"}},"i":0}}]}' id="mwwQ">−22 to −40</td>
<td style="text-align:right;" about="#mwt102">−8 to −40</td>
<td about="#mwt103" typeof="mw:Transclusion" style="text-align:right;" data-mw='{"parts":["| ",{"template":{"target":{"wt":"convert","href":"./Template:Convert"},"params":{"1":{"wt":"1.2"},"2":{"wt":"to"},"3":{"wt":"0.1"},"4":{"wt":"g/m3"},"5":{"wt":"oz/cuyd"},"disp":{"wt":"table"}},"i":0}}]}' id="mwwg">1.2 to 0.1</td>
