<p id="mwBQ"><span typeof="mw:Nowiki mw:Transclusion" about="#mwt4" data-mw='{"parts":[{"template":{"target":{"wt":"pp-semi-vandalism","href":"./Template:Pp-semi-vandalism"},"params":{"small":{"wt":"yes"}},"i":0}}]}' id="mwBg"></span><link typeof="mw:Extension/indicator" about="#mwt4" data-mw='{"name":"indicator","attrs":{"name":"pp-default"},"body":{"extsrc":"[[File:Semi-protection-shackle.svg|20px|link=Wikipedia:Protection policy#semi|alt=Page semi-protected|This article is semi-protected due to vandalism]]"}}'/><link rel="mw:PageProp/Category" href="./Category:Wikipedia_pages_semi-protected_against_vandalism#Snow" about="#mwt4" id="mwBw"/></p>
<table class="infobox" style="width:22em;width:25em" about="#mwt7" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"Infobox material\n","href":"./Template:Infobox_material"},"params":{"name":{"wt":"Snow"},"image":{"wt":"CargoNet Di 12 Euro 4000 Lønsdal - Bolna.jpg"},"image_size":{"wt":"250px"},"alt":{"wt":""},"caption":{"wt":"Norwegian train plowing through drifted snow"},"type":{"wt":""},"density":{"wt":"0.1–0.8 g/cm&lt;sup>3&lt;/sup>"},"radiation_resistance":{"wt":""},"uv_resistance":{"wt":""},"youngs_modulus":{"wt":""},"tensile_strength":{"wt":"1.5–3.5 kPa&lt;ref name = Snowenclyclopedia/>"},"elongation":{"wt":""},"compressive_strength":{"wt":"3–7 MPa&lt;ref name = Snowenclyclopedia/>"},"poissons_ratio":{"wt":""},"coeff_friction":{"wt":""},"melting_point":{"wt":"0 °C"},"heat_transfer_coeff":{"wt":""},"thermal_conductivity_note":{"wt":"For densities 0.1 to 0.5 g/cm&lt;sup>3&lt;/sup>&lt;!---http://www.the-cryosphere.net/7/217/2013/tc-7-217-2013.pdf--->"},"thermal_conductivity":{"wt":"0.05–0.7 W/(K·m)"},"thermal_diffusivity_note":{"wt":""},"thermal_diffusivity":{"wt":""},"linear_expansion":{"wt":""},"specific_heat":{"wt":""},"dielectric_constant_note":{"wt":"For dry snow density 0.1 to 0.9 g/cm&lt;sup>3&lt;/sup>&lt;!----http://www.dtic.mil/dtic/tr/fulltext/u2/p000148.pdf---->"},"dielectric_constant":{"wt":"1–3.2"},"footnotes":{"wt":"The physical properties of snow vary considerably from event to event, sample to sample, and over time."}},"i":0}}]}' id="mwCA"><tbody><tr><th colspan="2" style="text-align:center;font-size:125%;font-weight:bold;background:#ccc">Snow</th></tr><tr><td colspan="2" style="text-align:center"><figure-inline typeof="mw:Image"><a href="./File:CargoNet_Di_12_Euro_4000_Lønsdal_-_Bolna.jpg"><img resource="./File:CargoNet_Di_12_Euro_4000_Lønsdal_-_Bolna.jpg" src="//upload.wikimedia.org/wikipedia/commons/thumb/1/12/CargoNet_Di_12_Euro_4000_L%C3%B8nsdal_-_Bolna.jpg/250px-CargoNet_Di_12_Euro_4000_L%C3%B8nsdal_-_Bolna.jpg" data-