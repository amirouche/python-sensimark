<td style="text-align:right;" about="#mwt100">14 to −8</td>
<td about="#mwt101" typeof="mw:Transclusion" style="text-align:right;" data-mw='{"parts":["| ",{"template":{"target":{"wt":"convert","href":"./Template:Convert"},"params":{"1":{"wt":"1.2"},"2":{"wt":"to"},"3":{"wt":"1.4"},"4":{"wt":"g/m3"},"5":{"wt":"oz/cuyd"},"disp":{"wt":"table"}},"i":0}}]}' id="mwuw">1.2 to 1.4</td>
