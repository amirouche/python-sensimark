<p id="mwBQ"><span typeof="mw:Nowiki mw:Transclusion" about="#mwt3" data-mw='{"parts":[{"template":{"target":{"wt":"Use dmy dates","href":"./Template:Use_dmy_dates"},"params":{"date":{"wt":"September 2017"}},"i":0}}]}' id="mwBg"></span><link rel="mw:PageProp/Category" href="./Category:Use_dmy_dates_from_September_2017" about="#mwt3" id="mwBw"/>
