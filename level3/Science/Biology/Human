vid"},"last4":{"wt":"Mackaye"},"first4":{"wt":"Hassane Taisso"},"last5":{"wt":"Likius"},"first5":{"wt":"Andossa"},"last6":{"wt":"Ahounta"},"first6":{"wt":"Djimdoumalbaye"},"last7":{"wt":"Beauvilain"},"first7":{"wt":"Alain"},"last8":{"wt":"Blondel"},"first8":{"wt":"Cécile"},"last9":{"wt":"Bocherens"},"first9":{"wt":"Hervé"},"last10":{"wt":"Boisserie"},"first10":{"wt":"Jean-Renaud"},"last11":{"wt":"De Bonis"},"first11":{"wt":"Louis"},"last12":{"wt":"Coppens"},"first12":{"wt":"Yves"},"last13":{"wt":"Dejax"},"first13":{"wt":"Jean"},"last14":{"wt":"Denys"},"first14":{"wt":"Christiane"},"last15":{"wt":"Duringer"},"first15":{"wt":"Philippe"},"last16":{"wt":"Eisenmann"},"first16":{"wt":"Véra"},"last17":{"wt":"Fanone"},"first17":{"wt":"Gongdibé"},"last18":{"wt":"Fronty"},"first18":{"wt":"Pierre"},"last19":{"wt":"Geraads"},"first19":{"wt":"Denis"},"last20":{"wt":"Lehmann"},"first20":{"wt":"Thomas"},"last21":{"wt":"Lihoreau"},"first21":{"wt":"Fabrice"},"last22":{"wt":"Louchart"},"first22":{"wt":"Antoine"},"last23":{"wt":"Mahamat"},"first23":{"wt":"Adoum"},"last24":{"wt":"Merceron"},"first24":{"wt":"Gildas"},"last25":{"wt":"Mouchelin"},"first25":{"wt":"Guy"},"last26":{"wt":"Otero"},"first26":{"wt":"Olga"},"last27":{"wt":"Campomanes"},"first27":{"wt":"Pablo Pelaez"},"last28":{"wt":"De Leon"},"first28":{"wt":"Marcia Ponce"},"last29":{"wt":"Rage"},"first29":{"wt":"Jean-Claude"},"last30":{"wt":"Sapanet"},"first30":{"wt":"Michel"},"last31":{"wt":"Schuster"},"first31":{"wt":"Mathieu"},"last32":{"wt":"Sudre"},"first32":{"wt":"Jean"},"last33":{"wt":"Tassy"},"first33":{"wt":"Pascal"},"last34":{"wt":"Valentin"},"first34":{"wt":"Xavier"},"last35":{"wt":"Vignaud"},"first35":{"wt":"Patrick"},"last36":{"wt":"Viriot"},"first36":{"wt":"Laurent"},"last37":{"wt":"Zazzo"},"first37":{"wt":"Antoine"},"last38":{"wt":"Zollikofer"},"first38":{"wt":"Christoph"},"title":{"wt":"A new hominid from the Upper Miocene of Chad, Central Africa"},"journal":{"wt":"Nature"},"volume":{"wt":"418"},"issue":{"wt":"68