<p about="#mwt6" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"Pp-semi","href":"./Template:Pp-semi"},"params":{"small":{"wt":"yes"}},"i":0}},"\n",{"template":{"target":{"wt":"pp-move-indef","href":"./Template:Pp-move-indef"},"params":{},"i":1}},"\n",{"template":{"target":{"wt":"Use dmy dates","href":"./Template:Use_dmy_dates"},"params":{"date":{"wt":"July 2014"}},"i":2}},"\n",{"template":{"target":{"wt":"speciesbox\n","href":"./Template:Speciesbox"},"params":{"name":{"wt":"Cattle"},"image":{"wt":"CH cow 2 cropped.jpg"},"image_upright":{"wt":"1.1"},"image_caption":{"wt":"A Swiss [[Braunvieh]] cow wearing a [[cowbell]]"},"status":{"wt":"DOM"},"genus":{"wt":"Bos"},"species":{"wt":"taurus"},"authority":{"wt":"[[Carl Linnaeus|Linnaeus]], [[10th edition of Systema Naturae|1758]]"},"subdivision_ranks":{"wt":"Subspecies"},"subdivision":{"wt":"*&apos;&apos;[[Bos taurus indicus]]&apos;&apos;\n*&apos;&apos;[[Bos taurus taurus]]&apos;&apos;"},"synonyms":{"wt":"* &apos;&apos;Bos primigenius&apos;&apos;\n* &apos;&apos;Bos indicus&apos;&apos;\n* &apos;&apos;Bos longifrons&apos;&apos;"},"range_map":{"wt":"Bovine range-2013-14-02.png"},"range_map_caption":{"wt":"Bovine range"}},"i":3}}]}' id="mwBA"><link typeof="mw:Extension/indicator" about="#mwt140" data-mw='{"name":"indicator","attrs":{"name":"pp-default"},"body":{"extsrc":"[[File:Semi-protection-shackle.svg|20px|link=Wikipedia:Protection policy#semi|alt=Page semi-protected|This article is semi-protected.]]"}}'/><link rel="mw:PageProp/Category" href="./Category:Wikipedia_indefinitely_semi-protected_pages#Cattle"/>
