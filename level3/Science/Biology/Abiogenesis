ries"},"location":{"wt":"Pleasantville, NY"},"publisher":{"wt":"[[Reader&apos;s Digest Association]]"},"isbn":{"wt":"978-0-340-13000-1"},"lccn":{"wt":"69010418"},"ref":{"wt":"harv"}},"i":42}},"\n* ",{"template":{"target":{"wt":"cite book ","href":"./Template:Cite_book"},"params":{"last":{"wt":"Yarus"},"first":{"wt":"Michael"},"year":{"wt":"2010"},"title":{"wt":"Life from an RNA World: The Ancestor Within"},"location":{"wt":"Cambridge, MA"},"publisher":{"wt":"[[Harvard University Press]]"},"isbn":{"wt":"978-0-674-05075-4"},"lccn":{"wt":"2009044011"},"ref":{"wt":"harv"}},"i":43}},"\n* ",{"template":{"target":{"wt":"cite journal ","href":"./Template:Cite_journal"},"params":{"last1":{"wt":"Arrhenius"},"first1":{"wt":"Gustaf O."},"last2":{"wt":"Sales"},"first2":{"wt":"Brian C."},"last3":{"wt":"Mojzsis"},"first3":{"wt":"Stephen J."},"last4":{"wt":"Lee"},"first4":{"wt":"T."},"display-authors":{"wt":"3"},"date":{"wt":"21 August 1997"},"title":{"wt":"Entropy and Charge in Molecular Evolution – the Case of Phosphate"},"url":{"wt":"http://isotope.colorado.edu/~mojzsis/1997_Arrhenius_JTB.pdf"},"journal":{"wt":"[[Journal of Theoretical Biology]]"},"volume":{"wt":"187"},"issue":{"wt":"4"},"pages":{"wt":"503–522"},"doi":{"wt":"10.1006/jtbi.1996.0385"},"pmid":{"wt":"9299295"}},"i":44}},"\n* ",{"template":{"target":{"wt":"cite journal ","href":"./Template:Cite_journal"},"params":{"last":{"wt":"Cavalier-Smith"},"first":{"wt":"Thomas"},"authorlink":{"wt":"Thomas Cavalier-Smith"},"date":{"wt":"June 2006"},"title":{"wt":"Cell evolution and Earth history: stasis and revolution"},"journal":{"wt":"[[Philosophical Transactions of the Royal Society B]]"},"volume":{"wt":"361"},"issue":{"wt":"1470"},"pages":{"wt":"969–1006"},"doi":{"wt":"10.1098/rstb.2006.1842"},"pmid":{"wt":"16754610"},"pmc":{"wt":"1578732"}},"i":45}},"\n* ",{"template":{"target":{"wt":"cite journal ","href":"./Template:Cite_journal"},"params":{"last1":{"wt":"Fernando"},"first1":{"wt":"Chrisantha T."},"last2":{"wt":"Rowe"},"f