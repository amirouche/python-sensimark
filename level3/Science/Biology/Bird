<link rel="mw-deduplicated-inline-style" href="mw-data:TemplateStyles:r853264556" about="#mwt1655" typeof="mw:Extension/templatestyles" data-mw='{"name":"templatestyles","attrs":{"src":"Clade/styles.css"},"body":{"extsrc":""}}'/>
