)"},"population_estimate":{"wt":"49,996,445&lt;ref name=\"DANE clock\">{{cite web |url= http://www.dane.gov.co/reloj/ |title= Animated clock |publisher= Departamento Administrativo Nacional de Estadística (DANE) |accessdate= 8 May 2018 |archive-url= https://web.archive.org/web/20180116020433/http://www.dane.gov.co/reloj/ |archive-date= 16 January 2018 |dead-url= yes |df= dmy-all }}&lt;/ref>"},"population_census":{"wt":"42,888,592&lt;ref name=\"DANE clock\"/>"},"population_estimate_year":{"wt":"May 2018"},"population_estimate_rank":{"wt":"29th"},"population_census_year":{"wt":"2005"},"population_density_km2":{"wt":"40.74"},"population_density_rank":{"wt":"173rd"},"population_density_sq_mi":{"wt":"105.72&lt;!--Do not remove per [[WP:MOSNUM]]-->"},"GDP_PPP":{"wt":"$791.995&amp;nbsp;billion&lt;ref name=imf2>{{cite web |url= https://www.imf.org/external/pubs/ft/weo/2018/02/weodata/weorept.aspx?pr.x=55&amp;pr.y=5&amp;sy=2019&amp;ey=2023&amp;scsm=1&amp;ssd=1&amp;sort=country&amp;ds=.&amp;br=1&amp;c=233&amp;s=NGDPD%2CPPPGDP%2CNGDPDPC%2CPPPPC&amp;grp=0&amp;a= |title=World Economic Outlook Database: Colombia |publisher=International Monetary Fund |date= October 2018|accessdate= 15 February 2019}}&lt;/ref>"},"GDP_PPP_year":{"wt":"2019"},"GDP_PPP_rank":{"wt":"31st"},"GDP_PPP_per_capita":{"wt":"$15,719&lt;ref name=imf2/>"},"GDP_PPP_per_capita_rank":{"wt":""},"GDP_nominal":{"wt":"$355.163&amp;nbsp;billion&lt;ref name=imf2/>"},"GDP_nominal_year":{"wt":"2019"},"GDP_nominal_rank":{"wt":"32nd"},"GDP_nominal_per_capita":{"wt":"$7,049&lt;ref name=imf2/>"},"GDP_nominal_per_capita_rank":{"wt":""},"Gini":{"wt":"50.8 &lt;!--number only-->"},"Gini_year":{"wt":"2017"},"Gini_change":{"wt":"decrease &lt;!--increase/decrease/steady-->"},"Gini_ref":{"wt":"&lt;ref name=\"socio-economic policies\">{{cite web |url= https://www.dane.gov.co/files/investigaciones/condiciones_vida/pobreza/bol_pobreza_17.pdf|title= socio-economic policies|publisher= dane.gov.co |accessdate= 4 April 2018}}&lt;/ref>"},"HD