onomy"},"title":{"wt":"Rice in Vietnamese Culture and Economy"},"journal":{"wt":"Mario Einaudi Center for International Studies - Southeast Asia Program"},"ref":{"wt":"harv"}},"i":85}},"\n* ",{"template":{"target":{"wt":"cite journal\n  ","href":"./Template:Cite_journal"},"params":{"last":{"wt":"Van Van"},"first":{"wt":"Hoang"},"url":{"wt":"http://r-cube.ritsumei.ac.jp/repo/repository/rcube/4129/LCS_22_1pp7-18_HOANG.pdf"},"title":{"wt":"The Current Situation and Issues of the Teaching of English in Vietnam"},"publisher":{"wt":"[[Ritsumeikan Asia Pacific University]]"},"volume":{"wt":"22"},"issue":{"wt":"1"},"archiveurl":{"wt":"https://web.archive.org/web/20181012055826/http://r-cube.ritsumei.ac.jp/repo/repository/rcube/4129/LCS_22_1pp7-18_HOANG.pdf"},"archivedate":{"wt":"12 October 2018"},"deadurl":{"wt":"yes"},"ref":{"wt":"harv"}},"i":86}},"\n",{"template":{"target":{"wt":"refend","href":"./Template:Refend"},"params":{},"i":87}}]}' id="mwCeA"/><div class="refbegin   columns references-column-width" style="-moz-column-width: 30em; -webkit-column-width: 30em; column-width: 30em;" about="#mwt2275" id="mwCeE">
