<div style="clear:both;" about="#mwt722" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"Clear","href":"./Template:Clear"},"params":{},"i":0}}]}' id="mwBSk"></div>

</section><section data-mw-section-id="31" id="mwBSo"><h3 id="Religion">Religion</h3>
<div role="note" class="hatnote navigation-not-searchable" about="#mwt723" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"main","href":"./Template:Main"},"params":{"1":{"wt":"Religion in Vietnam"}},"i":0}}]}' id="mwBSs">Main article: <a rel="mw:WikiLink" href="./Religion_in_Vietnam" title="Religion in Vietnam">Religion in Vietnam</a></div>
<div class="PieChartTemplate thumb tright" style="" about="#mwt724" typeof="mw:Transclusion" data-mw='{"parts":[{"template":{"target":{"wt":"Pie chart\n","href":"./Template:Pie_chart"},"params":{"caption":{"wt":"Religion in Vietnam (2014){{sfn|Bielefeldt|2014}}"},"label1":{"wt":"[[Vietnamese folk religion]] or not religious population"},"value1":{"wt":"73.2"},"color1":{"wt":"#E0FFFF"},"label2":{"wt":"[[Buddhism]]"},"value2":{"wt":"12.2"},"color2":{"wt":"#FFDEAD"},"label3":{"wt":"[[Catholicism]]"},"value3":{"wt":"6.8"},"color3":{"wt":"#DDA0DD"},"label4":{"wt":"[[Caodaism]]"},"value4":{"wt":"4.8"},"color4":{"wt":"#FF69B4"},"label5":{"wt":"[[Protestantism]]"},"value5":{"wt":"1.5"},"color5":{"wt":"#87CEFA"},"label6":{"wt":"[[Hòa Hảo|Hoahaoism]]"},"value6":{"wt":"1.4"},"color6":{"wt":"#FFF8DC"},"label7":{"wt":"Others"},"value7":{"wt":"0.1"},"color7":{"wt":"#F0FFF0"}},"i":0}}]}' id="mwBSw"><div class="thumbinner" style="width:202px">
