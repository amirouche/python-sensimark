edro"},"last3":{"wt":"Mormina"},"first3":{"wt":"Maru"},"last4":{"wt":"Macaulay"},"first4":{"wt":"Vincent"},"last5":{"wt":"Clarke"},"first5":{"wt":"Dougie"},"last6":{"wt":"Blumbach"},"first6":{"wt":"Petya B."},"last7":{"wt":"Vizuete-Forster"},"first7":{"wt":"Matthieu"},"last8":{"wt":"Forster"},"first8":{"wt":"Peter"},"last9":{"wt":"Bulbeck"},"first9":{"wt":"David"},"date":{"wt":"January 2007"},"title":{"wt":"A Mitochondrial Stratigraphy for Island Southeast Asia"},"journal":{"wt":"The American Journal of Human Genetics"},"volume":{"wt":"80"},"issue":{"wt":"1"},"pages":{"wt":"29–43"},"doi":{"wt":"10.1086/510412"},"pmc":{"wt":"1876738"},"pmid":{"wt":"17160892"},"ref":{"wt":"harv"},"last1":{"wt":"Hill"},"first1":{"wt":"Catherine"},"last10":{"wt":"Oppenheimer"},"first10":{"wt":"Stephen"},"last11":{"wt":"Richards"},"first11":{"wt":"Martin"}},"i":12}},"\n* ",{"template":{"target":{"wt":"cite journal ","href":"./Template:Cite_journal"},"params":{"title":{"wt":"The earliest eyewitness accounts of the Formosan aborigines"},"given":{"wt":"Lawrence G."},"surname":{"wt":"Thompson"},"journal":{"wt":"Monumenta Serica"},"volume":{"wt":"23"},"pages":{"wt":"163–204"},"year":{"wt":"1964"},"jstor":{"wt":"40726116"},"ref":{"wt":"harv"},"doi":{"wt":"10.1080/02549948.1964.11731044"}},"i":13}},"\n* ",{"template":{"target":{"wt":"cite book ","href":"./Template:Cite_book"},"params":{"title":{"wt":"Formosa under the Dutch: described from contemporary records, with explanatory notes and a bibliography of the island"},"last":{"wt":"Valentijn"},"first":{"wt":"François"},"publisher":{"wt":"Kegan Paul"},"year":{"wt":"1903"},"editor-last":{"wt":"Campbell"},"editor-first":{"wt":"William"},"location":{"wt":"London"},"pages":{"wt":"25–75"},"chapter":{"wt":"History of the Dutch Trade"},"oclc":{"wt":"644323041"},"ref":{"wt":"harv"},"orig-year":{"wt":"First published 1724 in &apos;&apos;Oud en Nieuw Oost-Indiën&apos;&apos;"},"authorlink":{"wt":"François Valentijn"},"chapterurl":{"wt":"https://archive.org