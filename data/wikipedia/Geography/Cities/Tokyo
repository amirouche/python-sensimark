ate":{"wt":""},"parts_type":{"wt":"Divisions"},"parts":{"wt":"[[Special wards of Tokyo|23 special wards]], 26 cities, 1 district, and 4 subprefectures"},"government_type":{"wt":"[[Prefectures of Japan|Metropolis]]"},"leader_title":{"wt":"[[List of prefectural governors in Japan|Governor]]"},"leader_name":{"wt":"[[Yuriko Koike]] ([[Tomin First no Kai|TF]])"},"leader_title1":{"wt":"[[List of capitals in Japan|Capital]]"},"leader_name1":{"wt":"[[Special wards of Tokyo|Tokyo]]&lt;ref>{{cite web |url=http://www.chijihon.metro.tokyo.jp/sonota/syozaiti.htm |script-title=ja:都庁は長野市 |publisher=Tokyo Metropolitan Government |title=Archived copy |access-date=April 12, 2014 |archive-url=https://web.archive.org/web/20140419085331/http://www.chijihon.metro.tokyo.jp/sonota/syozaiti.htm |archive-date=April 19, 2014 |dead-url=yes |df=mdy-all }} Shinjuku is the location of the Tokyo Metropolitan Government Office. But Tokyo is not a \"municipality\". Therefore, for the sake of convenience, the notation of prefectural is \"Tokyo\".&lt;/ref>"},"leader_title2":{"wt":"[[House of Representatives (Japan)|Representatives]]"},"leader_name2":{"wt":"[[Tokyo&apos;s Diet electoral districts#House of Representatives|42]]"},"leader_title3":{"wt":"[[House of Councillors (Japan)|Councillors]]"},"leader_name3":{"wt":"[[Tokyo&apos;s Diet electoral districts#House of Councillors|11]]"},"area_total_km2":{"wt":"2187.66"},"area_land_km2":{"wt":""},"area_water_km2":{"wt":""},"area_water_percent":{"wt":"1.0"},"area_metro_km2":{"wt":"13572"},"area_rank":{"wt":"45th"},"elevation_m":{"wt":"40"},"population_total":{"wt":"13,839,910"},"population_as_of":{"wt":"September 2018"},"population_footnotes":{"wt":"&lt;ref name=\"toukei-metro\">{{cite web|url=http://www.toukei.metro.tokyo.jp/jsuikei/js-index.htm |script-title=ja:東京都の人口（推計）– Population of Tokyo (estimate)|publisher=Tokyo Metropolitan Government Bureau of Statistics Department |accessdate=October 22, 2018|deadurl=no|archiveurl=https://web.archive.org/web/20181002162446/http://www.toukei.metro.tokyo.jp/jsuikei/js-index.htm|archivedate=October 2, 2018}}&lt;/ref>"},"population_density_km2":{"wt":"6224.66"},"population_metro":{"wt":"38,140,000 – [[Greater Tokyo Area]]"},"population_metro_footnotes":{"wt":"&lt;ref name=\"UN-World-Cities-2016\">{{cite web|url=https://www.un.org/en/development/desa/population/publications/pdf/urbanization/the_worlds_cities_in_2016_data_booklet.pdf|title=The World&apos;s Cities in 2016|last=United Nations|date=March 12, 2017|website=United Nations|access-date=}}&lt;/ref>"},"population_density_metro_km2":{"wt":"2662"},"population_blank1_title":{"wt":"23 Wards"},"population_blank1":{"wt":"8,967,665"},"population_demonym":{"wt":"{{lang|ja|江戸っ子}} (Edokko), {{lang|ja|東京人}} (Tōkyō-jin), {{lang|ja|東京っ子}} (Tōkyōkko), Tokyoite"},"population_note":{"wt":"(2015 per prefectural government)"},"demographics_type1":{"wt":"GDP"},"demographics1_footnotes":{"wt":"&amp;nbsp;(Nominal; 2015)&lt;ref>{{cite web|url= http://www.metro.tokyo.jp/tosei/hodohappyo/press/2018/03/28/18.html |script-title=ja:都民経済計算（都内総生産等）平成27年度年報\n|website= www.metro.tokyo.jp}}&lt;/ref>&lt;ref>{{cite web|url= https://www.ofx.com/en-au/forex-news/historical-exchange-rates/ |title= Yearly Average Rates |publisher= OFX|deadurl= yes |archiveurl= https://web.archive.org/web/20150316233916/https://www.ofx.com/en-au/forex-news/historical-exchange-rates/ |archivedate= March 16, 2015 }}&lt;/ref>&lt;ref>{{cite web|url= http://www.stat.go.jp/data/kokusei/2015/kekka/kihon1/pdf/gaiyou1.pdf|title= 2015 Japan Population}}&lt;/ref>"},"demographics1_title1":{"wt":"Total"},"postal_code_type":{"wt":"[[ISO 3166-2:JP|ISO 3166-2]]"},"postal_code":{"wt":"JP-13"},"area_code":{"wt":""},"blank_name_sec2":{"wt":"Flower"},"blank_info_sec2":{"wt":"[[Prunus × yedoensis|Somei-Yoshino]] cherry blossom"},"blank1_name_sec2":{"wt":"Tree"},"blank1_info_sec2":{"wt":"[[Ginkgo biloba|Ginkgo tree]] (&apos;&apos;Ginkgo biloba&apos;&apos;)"},"website":{"wt":"{{URL| http://www.metr